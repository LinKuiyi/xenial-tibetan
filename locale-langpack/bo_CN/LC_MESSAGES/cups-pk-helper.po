msgid "Add/Remove/Edit a class"
msgstr "རིགས་ཁ་སྣོན/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Add/Remove/Edit a local printer"
msgstr "རང་སའི་པར་འཁོར་ཁ་སྣོན/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Add/Remove/Edit a printer"
msgstr "པར་འཁོར་ཁ་སྣོན/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Add/Remove/Edit a remote printer"
msgstr "རྒྱང་རིང་པར་འཁོར་སྣོན་པ/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Change printer settings"
msgstr "པར་འཁོར་སྒྲིག་འགོད་བཅོས་པ།"

msgid "Enable/Disable a printer"
msgstr "པར་འཁོར་མགོ་སློང་/འགོག་པ།"

msgid "Get list of available devices"
msgstr "སྤྱོད་རུང་སྒྲིག་ཆས་རེའུ་མིག་ཐོབ་པ།"

msgid "Get/Set server settings"
msgstr "ཞབས་ཞུ་ཆས་སྒྲིག་འགོད་ཐོབ་པ/སྒྲིག་འགོད"

msgid "Privileges are required to add/remove/edit a class."
msgstr "དབང་སྤྲད་ནས་རིགས་སྣོན་པ/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Privileges are required to add/remove/edit a local printer."
msgstr "དབང་སྤྲད་ནས་རང་སའི་པར་འཁོར་སྣོན་པ/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Privileges are required to add/remove/edit a printer."
msgstr "དབང་སྤྲད་ནས་པར་འཁོར་སྣོན་པ/སྤོ་བ/རྩོམ་སྒྲིག"

msgid "Privileges are required to add/remove/edit a remote printer."
msgstr "དབང་སྤྲད་ནས་རྒྱང་རིང་པར་འཁོར་སྣོན་པ/སྤོ་བ/རྩོམ་སྒྲིག"

msgid ""
"Privileges are required to change printer settings. This should only be "
"needed from the Printers system settings panel."
msgstr ""
"དབང་སྤྲད་ནས་པར་འཁོར་གྱི་སྒྲིག་འགོད་བཅོས་དགོས། "
"པར་འཁོར་རྒྱུད་ཁོངས་ནས་ངོས་པང་ལག་བསྟར་བཀོལ་སྤྱོད་བྱེད་སྐབས་ཁོ་ནར་དགོས་པ།"

msgid "Privileges are required to enable/disable a printer, or a class."
msgstr "དབང་སྤྲད་ནས་པར་འཁོར་འགོ་སློང་/འགོག་པ།"

msgid "Privileges are required to get list of available devices."
msgstr "དབང་སྤྲད་ནས་སྤྱོད་རུང་སྒྲིག་ཆས་རེའུ་མིག་ཐོབ་པ།"

msgid "Privileges are required to get/set server settings."
msgstr "དབང་སྤྲད་ནས་ཞབས་ཞུ་ཆས་སྒྲིག་འགོད་ཐོབ་པ/སྒྲིག་འགོད།"

msgid ""
"Privileges are required to restart/cancel/edit a job owned by another user."
msgstr ""
"དབང་སྤྲད་ནས་སྤྱོད་མཁན་གཞན་དག་ལ་ཡོད་པའི་ལས་འགན་འགོ་བསྐྱར་སློང་/ལེན་པ་/རྩོམ་སྒྲིག"

msgid "Privileges are required to restart/cancel/edit a job."
msgstr "དབང་སྤྲད་ནས་ལས་འགན་འགོ་བསྐྱར་སློང་/ལེན་པ་/རྩོམ་སྒྲིག"

msgid ""
"Privileges are required to set a printer, or a class, as default printer."
msgstr "དབང་སྤྲད་ནས་པར་འཁོར། རིགས་ཡང་ན་སོར་བཞག་པར་འཁོར་སྒྲིག་འགོད།"

msgid "Restart/Cancel/Edit a job"
msgstr "ལས་འགན་བསྐྱར་སློང་/ལེན་པ་/རྩོམ་སྒྲིག"

msgid "Restart/Cancel/Edit a job owned by another user"
msgstr "སྤྱོད་མཁན་གཞན་དག་གི་ལས་འགན་བསྐྱར་སློང་/ལེན་པ་/རྩོམ་སྒྲིག"

msgid "Set a printer as default printer"
msgstr "པར་འཁོར་དེ་སོར་བཞག་པར་འཁོར་ལ་སྒྲིག་འགོད།"
