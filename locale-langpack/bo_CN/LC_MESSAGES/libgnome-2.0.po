msgid "Allow postponing of breaks"
msgstr "དུས་འགྱངས་མཚམས་ཆད་བྱེད་ཆོག"

msgid "App version"
msgstr "བྱ་རིམ་པར་གཞི།"

msgid "Basename of the default theme used by gtk+."
msgstr "gtk+སྤྱད་པའི་བརྗོད་སྙིང་གི་རྨང་མིང་། "

msgid "Beep when a modifier is pressed."
msgstr "རྒྱན་མཐེབ་གནོན་སྐབས་་“སྦི”་སྒྲ་གྲགས་པ།"

msgid "Browser needs terminal"
msgstr "བཤར་ཆས་ལ་མཐའ་སྣེ་དགོས།"

msgid "Browser understands remote"
msgstr "བཤར་ཆས་ཀྱིས་རྒྱང་རིང་བཀའ་བརྡར་གོ་བ་ལོན་པ།"

msgid "Buttons Have Icons"
msgstr "མཐེབ་གནོན་ལ་རིས་རྟགས་ལྡན་པ།"

msgid "Calendar needs terminal"
msgstr "ལོ་རྩིས་ལ་མཐའ་སྣེ་དགོས།"

msgid "Can Change Accels"
msgstr "མྱུར་མཐེབ་བསྒྱུར་ཆོག"

msgid "Color Shading Type"
msgstr "མདོག་རིམ་འགྱུར་གྱི་རིགས་གྲས།"

msgid "Create Directories"
msgstr "དཀར་ཆག་བཟོ་བ།"

msgid "Cursor Blink"
msgstr "འོད་རྟགས་འོད་འཚེར།"

msgid "Cursor Blink Time"
msgstr "འོད་རྟགས་འཚེར་བའི་དུས་ཚོད།"

msgid "Cursor font"
msgstr "འོད་རྟགས་ཡིག་གཟུགས།"

msgid "Cursor size"
msgstr "འོད་རྟགས་ཀྱི་ཆེ་ཆུང་།"

msgid "Cursor theme"
msgstr "འོད་རྟགས་ཀྱི་བརྗོད་གཞི།"

msgid "Default browser"
msgstr "སོར་བཞག་བཤར་ཆས།"

msgid "Default browser for all URLs."
msgstr "URLཡོངས་ཀྱི་སོར་བཞག་བཤར་ཆས།"

msgid "Default calendar"
msgstr "སོར་བཞག་ལོ་ཐོ།"

msgid "Default font"
msgstr "སོར་བཞག་ཡིག་གཟུགས།"

msgid "Default mixer device"
msgstr "སོར་བཞག་སྒྲ་བསྲེས་སྒྲིག་ཆས།"

msgid "Default mixer tracks"
msgstr "སོར་བཞག་སྒྲ་བསྲེས་སྒྲིག་ཆས་ཀྱི་སྒྲ་ལམ།"

msgid "Default tasks"
msgstr "སོར་བཞག་ལས་འགན།"

msgid "Default tasks application"
msgstr "སོར་བཞག་ལས་འགན་ཉེར་སྤྱོད་བྱ་རིམ།"

msgid "Disable URL and MIME type handlers"
msgstr " URL དང་ MIME རིགས་ཀྱི་སྒྲིག་གཅོད་ཆས་སྤྱོད་མི་ཆོག"

msgid "Disable all external thumbnailers"
msgstr "ཕྱི་རོལ་གྱི་བསྡུས་རིས་བྱ་རིམ་ཡོད་ཚད་སྤྱོད་མི་ཆོག་པ།"

msgid "Disable command line"
msgstr "་བཀའ་བརྡའི་ཕྲེང་སྤྱོད་མི་ཆོག་པ།"

msgid "Disable lock screen"
msgstr "བརྙན་ཡོལ་དམིགས་འཛིན་སྤྱད་མི་ཆོག"

msgid "Disable print setup"
msgstr "པར་འདེབས་སྒྲིག་འགོད་སྤྱད་མི་ཆོག"

msgid "Disable printing"
msgstr "པར་འདེབས་སྤྱོད་མི་ཆོག"

msgid "Disable saving files to disk"
msgstr "ཡིག་ཆ་སྡུད་སྡེར་ནང་ཉར་ཚགས་བྱེད་པ་སྤྱད་མི་ཆོག"

msgid "Disable user switching"
msgstr "སྤྱོད་མཁན་བརྗེ་རེས་སྤྱོད་མི་ཆོག"

msgid ""
"Distance in pixels the pointer must move before accelerated mouse motion is "
"activated. A value of -1 is the system default."
msgstr ""
"ཙིག་རྟགས་འགུལ་སྐྱོད་མགྱོགས་སུ་མ་བཏང་གོང་ཙིག་རྟགས་ངེས་པར་དུ་སྒུལ་བདེའི་བརྙན་རྒྱུའི་གྲངས་རྟགས་དགོས།"
" རྒྱུད་ཁོངས་གི་སོར་བཞག་ཐང་ནི་-1ཡིན།"

msgid "Document font"
msgstr "ཡིག་ཚགས་ཡིག་གཟུགས།"

msgid "Double Click Time"
msgstr "ཉིས་རྡེབ་དུས་ཚོད།"

msgid "Drag Threshold"
msgstr "འདྲུད་སྒུལ་མཚམས་ཐང་།"

msgid "Draw Desktop Background"
msgstr "ཅོག་ངོས་ཀྱི་རྒྱབ་ལྗོངས་འབྲི་བ།"

msgid "Enable Animations"
msgstr "འགུལ་རིས་འགོ་སློང་།"

msgid "Enable sound server startup."
msgstr "འགོ་སློང་སྐབས་སྐད་སྒྲའི་ཞབས་ཞུ་ཆས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "Error message"
msgstr " ནོར་འཁྲུལ་བརྡ་འཕྲིན།"

msgid "Exec Arguments"
msgstr "Execཞུགས་གྲངས།"

msgid "File Icon Theme"
msgstr "ཡིག་ཆའི་རིས་རྟགས་བརྗོད་སྙིང།"

msgid "File name of the bell sound to be played."
msgstr "གཏོང་བྱའི་དྲིལ་སྒྲའི་ཡིག་ཆའི་མིང་།"

msgid "File to use for the background image."
msgstr "རྒྱབ་ལྗོངས་པར་རིས་ལ་སྤྱད་པའི་ཡིག་ཆ།"

msgid "GTK IM Module"
msgstr "GTKའཇུག་ཐབས་དཔེ་དུམ།"

msgid "GTK IM Preedit Style"
msgstr "GTKའཇུག་ཐབས་ཀྱི་སྔོན་བསྒྲིགས་རྣམ་པ།"

msgid "GTK IM Status Style"
msgstr "GTKའཇུག་ཐབས་རྣམ་པའི་དཔེ་དབྱིབས།"

msgid "Gtk+ Theme"
msgstr "Gtk+བརྗོད་སྙིང།"

msgid "Help options"
msgstr "རོགས་རམ་འདེམས་ཚན།"

msgid ""
"Highlights the current location of the pointer when the Control key is "
"pressed and released."
msgstr ""
"Comlམཐེབ་མནན་ཏེ་གློད་སྐབས།ཙིག་རྟགས་ཀྱི་སྟོན་ཁབ་ཀྱི་མིག་སྔའི་གནས་བབ་གསལ་པོར་མངོན་ཡོད།"

msgid "How long to accelerate in milliseconds"
msgstr "ཧའོ་སྐར་ཆ་ག་ཚོད་ཀྱི་རྗེས་སུ་འགྲོས་སྣོན་བྱེད་འགོ་ཚུགས་པ།"

msgid "How many milliseconds it takes to go from 0 to maximum speed."
msgstr "ཧའོ་སྐར་ཆ་ཅི་ཙམ་བརྒྱུད་དེ་མྱུར་ཚད་མཐོ་ཤོས་སུ་སླེབས་པ། "

msgid ""
"How many milliseconds to wait before mouse movement keys start to operate."
msgstr ""
"ཧའོ་སྐར་ཆ་ཅི་ཙམ་སྒུག་རྗེས་ཙིག་རྟགས་སྒུལ་མཐེབ་ཀྱིས་ལས་ཀ་བྱེད་འགོ་ཚུགས་པ། "

msgid "How many pixels per second to move at the maximum speed."
msgstr "ཆེས་མགྱོགས་པའི་སྐབས་སུ་སྐར་ཆ་རེ་ལ་བརྙན་རྒྱུ་ག་ཚོད་རེ་སྤོ་བ། "

msgid "Icon Theme"
msgstr "རིས་རྟགས་བརྗོད་སྙིང་།"

msgid "Icon theme to use for the panel, nautilus etc."
msgstr "ངོས་པང་དང་nautilusསོགས་ཀྱི་རིས་རྟགས་བརྗོད་གཞི། "

msgid "Initial delay in milliseconds"
msgstr "ཐོག་མའི་ཕྱིར་འགྱངས།(ཧའོ་སྐར་ཆ།)"

msgid "Input feedback sounds"
msgstr "ཚུར་ལན་སྐད་འཇུག"

msgid "Keyboard Bell Custom Filename"
msgstr "རང་གིས་མཚན་ཉིད་བཞག་པའི་མཐེབ་གཞོང་གི་སྒྲ་དང་ཡིག་ཆའི་མིང་།"

msgid "Keyboard shortcut to open the menu bars."
msgstr "འདེམས་བྱང་སྡེའི་མཐེབ་གཞོང་མྱུར་མཐེབ་ཕྱེ་བ།"

msgid "Left or Top color when drawing gradients, or the solid color."
msgstr ""
"རིམ་འགྱུར།དེ་ནི་གཡོན་ངོས་སམ་ཡང་ན་སྟེང་ངོས་ཀྱི་ཁ་དོག་དང་།ཡང་ན་མདོག་གཙང་མ།"

msgid "Length of the cursor blink cycle, in milliseconds."
msgstr "འོད་རྟགས་འོད་འཚེར་བའི་འཁོར་ཡུན་རིང་ལ།སྡེ་ཚན་ནི་སྐར་ཆ་གཅིག་ཡིན།"

msgid "Locate Pointer"
msgstr "གནས་ངེས་སྟོན་ཁབ།"

msgid "Log in"
msgstr "ཐོ་འགོད།"

msgid "Log out"
msgstr "ཐོ་ཁོངས་ནས་བསུབ་པ།"

msgid ""
"Maximum age for thumbnails in the cache, in days. Set to -1 to disable "
"cleaning."
msgstr ""
"འགྱངས་གསོག་ཁྲོད་བསྡུས་རིས་གསོག་ཉར་ཚགས་ཀྱི་ཉིན་གྲངས། -1 "
"ལ་སྒྲིག་ནས་དུས་ཡོལ་གཙང་སེལ་བྱེད་པ་བཀག་འགོག་བྱེད་པ།"

msgid ""
"Maximum size of the thumbnail cache, in megabytes. Set to -1 to disable "
"cleaning."
msgstr ""
"རི་མོ་བསྡུས་བཅོས།ལྷོད་གསོག་གི་ཆེས་ཆེ་བའི་ཚད་གཞི་ནིMBཡིན། "
"(1）ཚད་བརྒལ་གཙང་བཤེར་བྱེད་པར་གཏན་འགོག་བྱ་དགོས།"

msgid "Menubar Detachable"
msgstr "འདེམས་བྱང་སྡེཁ་བྲལ་ཆོག་པ།"

msgid "Menubar accelerator"
msgstr "འདེམས་བྱང་སྡེའི་མྱུར་མཐེབ།"

msgid "Menus Have Icons"
msgstr "འདེམས་བྱང་ལ་རིས་རྟགས་ལྡན་པ།"

msgid "Menus Have Tearoff"
msgstr "འདེམས་བྱང་ལ་དབྱེ་ཐིག་ལྡན་པ།"

msgid "Minimum interval in milliseconds"
msgstr "ཆེས་ཆུང་བར་ཐག(ཧའོ་སྐར་ཆ།)"

msgid "Monospace font"
msgstr "ཞེང་མཚུངས་ཡིག་གཟུགས།"

msgid "Motion Threshold"
msgstr "ལུས་རྩལ་མཚམས་ཐང་།"

msgid "Mouse button orientation"
msgstr "ཙིག་རྟགས་མཐེབ་གནོན་ཁ་ཕྱོགས།"

msgid ""
"Name of a monospaced (fixed-width) font for use in locations like terminals."
msgstr "མཐའ་སྣེ་སོགས་སུ་བཀོལ་བའི་ཞེང་མཚུངས་ཡིག་གཟུགས་ཀྱི་མིང་།"

msgid "Name of the GTK+ input method Preedit Style used by gtk+."
msgstr "gtk+ལ་སྤྱོད་པའི་GTK+འཇུག་ཐབས་སྔོན་བཀོད་སྒྲིག་གཅོད་རྣམ་པའི་མིང་།"

msgid "Name of the GTK+ input method Status Style used by gtk+."
msgstr "gtk+ལ་སྤྱོད་པའི་GTK+འཇུག་ཐབས་རྣམ་པའི་དཔེ་དབྱིབས་ཀྱི་མིང་།"

msgid "Name of the default font used by gtk+."
msgstr "gtk+བཀོལ་བའི་སོར་བཞག་ཡིག་གཟུགས་ཀྱི་མིང་།"

msgid "Name of the default font used for reading documents."
msgstr "ཡིག་ཚགས་ཀློག་པར་བཀོལ་བའི་སོར་བཞག་ཡིག་གཟུགས་ཀྱི་མིང་།"

msgid "Name of the input method module used by GTK+."
msgstr "GTK+ཡིས་སྤྱད་པའི་སོར་བཞག་འཇུག་ཐབས་དཔེ་དུམ་ཀྱི་མིང་། "

msgid "Number of minutes of typing time before break mode starts."
msgstr "མཚམས་བཅད་རྣམ་པ་འགོ་མ་ཚུགས་གོང་གི་ཆེས་ཉུང་བའི་ཡི་གེ་གཏགས་པའི་དུས་ཚོད།"

msgid "Number of minutes that the typing break should last."
