msgid "<b>Input/Output</b>"
msgstr "<b>ནང་འཇུག/ཕྱིར་འདོན།</b>"

msgid "<b>Options</b>"
msgstr "<b>འདེམ་བྱའི་འདམ་གཞི།</b>"

msgid "Capture mouse event"
msgstr "ཙིག་རྟགས་བཟུང་བའི་དོན་རྐྱེན།"

msgid "Choose input method"
msgstr "འཇུག་ཐབས་འདེམ་པ།"

msgid "Choose output charset"
msgstr "ཕྱིར་འདོན་ཡིག་རྟགས་ཚོགས་འདེམ་པ།"

msgid "Enable Macro"
msgstr "རགས་སྤྱོད་འགོ་ཚུགས་པ།"

msgid "Enable _macro"
msgstr "རགས་སྤྱོད་འགོ་ཚུགས་པ།(_M)"

msgid "Enable _spell check"
msgstr "སྦྱོར་འབྲི་ཞིབ་བཤེར་འགོ་རྩོམ།(_S)"

msgid "Enable spell check"
msgstr "སྦྱོར་འབྲི་ཞིབ་བཤེར་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "IBus-Unikey Setup"
msgstr "IBus-Unikey སྒྲིག་འགོད།"

msgid "If enable, you can decrease mistake when typing"
msgstr ""
"བེད་སྤྱོད་བྱས་རྗེས་ཁྱེད་ཀྱིས་ཡི་གེ་བརྟག་སྐབས་ཀྱི་ནོར་འཁྲུལ་ཇེ་ཉུང་དུ་གཏོང་ཐུབ།"

msgid "Input method:"
msgstr "འཇུག་ཐབས།"

msgid "Options"
msgstr "འདེམས་ཚན།"

msgid "Options for Unikey"
msgstr "Unikeyའདེམས་བྱང་།"

msgid "Output charset: "
msgstr "ཡིག་རྟགས་ཚོགས་ཕྱིར་འདོན།"

msgid "Run Setup..."
msgstr "འཁོར་སྐྱོད་སྒྲིག་འགོད།"

msgid "Run setup utility for IBus-Unikey"
msgstr " IBus-Unikey ཡི་སྒྲིག་འགོད་དངོས་སྤྱོད་ཡོ་བྱད་འཁོར་སྐྱོད།"

msgid "_Edit macro"
msgstr "རྩོམ་སྒྲིག་རགས་པ།(_E)"
