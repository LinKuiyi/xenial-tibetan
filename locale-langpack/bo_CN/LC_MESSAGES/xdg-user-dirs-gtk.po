msgid "Current folder name"
msgstr "མིག་སྔའི་ཡིག་ཁུག་མིང་།"

msgid "New folder name"
msgstr "ཡིག་ཁུག་གསར་པའི་མིང་།"

msgid "Note that existing content will not be moved."
msgstr "མཉམ་ཞོག  ད་ཡོད་ཀྱི་ནང་དོན་མི་སྤོ།"

msgid "There was an error updating the folders"
msgstr "ཡིག་ཁུག་གསར་སྒྱུར་བྱེད་སྐབས་ནོར་འཁྲུལ་ཤོར་བ།"

msgid "Update common folders names to match current locale"
msgstr ""
"སྤྱི་པའི་ཡིག་ཁུག་གི་མིང་གསར་སྒྱུར་བྱེད་པར་མིག་སྔའི་ས་ཁོངས་སྒྲིག་བཀོད་དང་འཚམ་པར་བྱེད་དགོས།"

msgid "Update standard folders to current language?"
msgstr "ཚད་ལྡན་གྱི་ཡིག་ཁུག་མིག་སྔའི་སྐད་དུ་བསྒྱུར་རམ།"

msgid "User folders update"
msgstr "སྤྱོད་མཁན་གྱི་ཡིག་ཁུག་གསར་བརྗེ།"

msgid "_Don't ask me this again"
msgstr "ང་ལ་བསྐྱར་དུ་འདྲི་རྩད་མ་བྱེད།(_D)"

msgid "_Keep Old Names"
msgstr "མིང་རྙིང་པ་སོར་བཞག་བྱེད་པ།(_K)"

msgid "_Update Names"
msgstr "མིང་གསར་བསྒྱུར།(_U)"
