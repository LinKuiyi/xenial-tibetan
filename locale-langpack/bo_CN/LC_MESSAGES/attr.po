msgid "%s %s -- get extended attributes\n"
msgstr "%s %s --རྒྱ་སྐྱེད་གཏོགས་གཤིས་ཐོབ།\n"

msgid "%s %s -- set extended attributes\n"
msgstr "%s %s --རྒྱ་སྐྱེད་གཏོགས་གཤིས་བཀོད།\n"

msgid "%s: %s: No filename found in line %d, aborting\n"
msgstr "%s: %s: འཕྲེད་ %dརུ་ཡིག་ཆའི་མིང་མ་རྙེད། ཕྱིར་འདོན།\n"

msgid "%s: No filename found in line %d of standard input, aborting\n"
msgstr "%s:ཚད་ལྡན་ནང་འཇུག་གི་འཕྲེད་ %dརུ་ཡིག་ཆའི་མིང་མ་རྙེད། ཕྱིར་འདོན།\n"

msgid "%s: Removing leading '/' from absolute path names\n"
msgstr "%s: བལྟོས་མེད་འགྲོ་ལམ་མིང་མཇུག་ཏུ་ཨང་རྟགས\" / \"འདོར།"

msgid "%s: invalid regular expression \"%s\"\n"
msgstr ""
"%s:ནུས་མེད་ཀྱི་ཡང་དག་མཚོན་ཚུལ།\\%s\"\n"
"\""

msgid "-V only allowed with -s\n"
msgstr "-Vདང-sམ་གཏོགས་མཉམ་དུ་སྤྱོད་མི་ཐུབ།\n"

msgid "A filename to operate on is required\n"
msgstr "ཡིག་ཆའི་མིང་དགོས།\n"

msgid "At least one of -s, -g, -r, or -l is required\n"
msgstr "མ་མཐར་ཡང་ -s、-g、-r、-lནང་གི་གཅིག་དགོས།\n"

msgid "Attribute \"%s\" had a %d byte value for %s:\n"
msgstr ""
"གཏོགས་གཤིས\\%s\" ལ་ཡིག་ཚིགས %dནས %s:ཡོད།\n"
"\""

msgid "Attribute \"%s\" has a %d byte value for %s\n"
msgstr ""
"གཏོགས་གཤིས\\%s\"ལ་ཡིག་ཚིགས་ཚད%dནས%sཡོད།\n"
"\""

msgid "Attribute \"%s\" set to a %d byte value for %s:\n"
msgstr ""
"གཏོགས་གཤིས\\%s\" ཡིག་ཚིགས་ཀྱི་གྲངས་བྲིན་ %dནས་ %sལ་བཀོད།\n"
"\""

msgid "Could not get \"%s\" for %s\n"
msgstr ""
"ཐོབ་ཐབས་མེད་ \\%s\" ནས %s:\n"
"\""

msgid "Could not list \"%s\" for %s\n"
msgstr ""
" \\%s\"ནས%sལ་འགྲེམས་མི་ཐུབ།\n"
"\""

msgid "Could not remove \"%s\" for %s\n"
msgstr ""
" \\%s\"ནས %s:ལ་འདོར་མི་ཐུབ།\n"
"\""

msgid "Could not set \"%s\" for %s\n"
msgstr "\\%s\"ནི་ %s\\བྱས་ནས་བཀོད་མི་ཐུབ།n\""

msgid "No such attribute"
msgstr "གཏོགས་གཤིས་འདི་མེད།"

msgid "Only one of -s, -g, -r, or -l allowed\n"
msgstr " -s,-g,-r,-lནང་གི་གཅིག་མ་གཏོགས་མི་རུང་།\n"

msgid "Unrecognized option: %c\n"
msgstr "མི་ཤེས་པའི་འདེམས་ཚན་%c\n"

msgid "Usage: %s %s\n"
msgstr "སྤྱོད་སྟངས %s %s\n"

msgid "getting attribute %s of %s"
msgstr "གཏོགས་གཤིས %sནི་ %sལས་ཐོབ།"

msgid "listing attributes of %s"
msgstr "%sཡི་གཏོགས་གཤིས་འགྲེམས།"

msgid "setting attribute %s for %s"
msgstr "གཏོགས་གཤིས %sནི་ %sལྟར་བཀོད།"

msgid "setting attributes for %s"
msgstr " %sལ་གཏོགས་གཤིས་བཀོད།"
