msgid ""
"The port on the machine defined by \"/system/proxy/ftp/host\" that you proxy"
" through."
msgstr ""
"ལས་ཚབ་བརྒྱུད་པའི་འཕྲུལ་ཆས་ཀྱི་མཐུད་ཁ་ནི/system/proxy/ftp/hostཡི་ནང་དུ་མཚན་ཉིད་འཇོག་དགོས།"
" "

msgid ""
"The port on the machine defined by \"/system/proxy/http/host\" that you "
"proxy through."
msgstr ""
"ལས་ཚབ་བརྒྱུད་པའི་འཕྲུལ་ཆས་ཀྱི་མཐུད་ཁ་ནི/system/proxy/http/hostཡི་ནང་དུ་མཚན་ཉིད་འཇོག་དགོས།"
" "

msgid ""
"The port on the machine defined by \"/system/proxy/https/host\" that you "
"proxy through."
msgstr ""
"ལས་ཚབ་བརྒྱུད་པའི་འཕྲུལ་ཆས་ཀྱི་མཐུད་ཁ་ནི/system/proxy/https/hostཡི་ནང་དུ་མཚན་ཉིད་འཇོག་དགོས།"
" "

msgid ""
"The port on the machine defined by \"/system/proxy/socks/host\" that you "
"proxy through."
msgstr ""
"ལས་ཚབ་བརྒྱུད་པའི་འཕྲུལ་ཆས་ཀྱི་མཐུད་ཁ་ནི/system/proxy/socks/hostཡི་ནང་དུ་མཚན་ཉིད་འཇོག་གི་ཡོད།"
" "

msgid ""
"The power of the magnification. A value of 1.0 means no magnification. A "
"value of 2.0 doubles the size."
msgstr ""
"ཆེར་སྐྱེད་ཚད། 1.0ཆེར་སྐྱེད་མི་བྱེད་པ་མཚོན། 2.0ལྡབ་གཉིས་ཀྱི་ཆེར་སྐྱེད། "

msgid ""
"The time delay before raising a window if auto-raise is set to true. The "
"delay is given in thousandths of a second."
msgstr ""
"གལ་ཏེ་རང་འགུལ་རིམ་སྤོར་་trueལ་སྒྲིག་ཚེ་སྒེའུ་ཁུང་ཡར་སྤོར་གྱི་འགྱངས་ཡུན། "
"དུས་ཚོད་འདི་ཧའོ་སྐར་ཆ་ཡིས་འཇལ་དགོས། "

msgid "Thickness of the crosshairs in pixels"
msgstr "རྒྱ་གྲམ་འོད་རྟགས་ཀྱི་ཐིག་གི་སྦོམ་ཕྲ། (བརྙན་རྒྱུས་བརྩི་བ།)"

msgid "This key is not used, and should not be read or modified."
msgstr "མཐེབ་གནོན་འདི་བཀོལ་མེད་པ་མ་ཟད། ཀློག་པའམ་ཡང་ན་བཟོ་བཅོས་བྱས་ན་མི་འགྲིག "

msgid ""
"This key is not used; HTTP proxying is enabled when the host key is non-"
"empty and the port is non-0."
msgstr ""
"མཐེབ་གནོན་འདི་བཀོལ་མེད་པ། "
"གཙོ་འཁོར་མཐེབ་སྟོང་པ་མིན་ལ་མཐུད་ཁ་0མིན་དུས་སུ་HTTPལས་ཚབ་སྤྱོད་འགོ་ཚུགས། "

msgid ""
"This key overrides the automatic hiding of the Universal Access status icon "
"when no accessibility features are enabled."
msgstr ""
"མཐེབ་འདིས་ལྟ་སྤྱོད་བྱེད་ནུས་སྤྱོད་འགོ་ཚུགས་མེད་པའི་སྐབས་རང་འགུལ་གྱིས་སྤྱི་སྤྱོད་ལྟ་སྤྱོད་རྣམ་པའི་རིས་རྟགས་སྦེད་པའི་སྒྲིག་འགོད་ཀྱི་ཚབ་བྱེད་པ།"
" "

msgid ""
"Thumbnails will not be created for files whose mime-type is contained in the"
" list."
msgstr "རེའུ་མིག་ནང་mimeཡོད་པའི་རིགས་ཀྱི་ཡིག་ཆ་ལ་བསྡུས་རིས་གསར་བཟོ་མི་བྱ།"

msgid "Time after which the cursor stops blinking, in seconds."
msgstr ""
"འོད་རྟགས་འཚེར་མཚམས་མ་བཞག་གོང་གི་སྒུག་པའི་དུས་ཚོད། སྡེ་ཚན་ནི་སྐར་ཆ་ཡིན། "

msgid "Time before locking"
msgstr "བརྙན་ཡོལ་སྒྲོག་བཙུད་དུས་འགྱངས།"

msgid "Time before logout option"
msgstr "རྩིས་ཐེམ་ནས་བསུབ་པའི་འདེམས་བྱང་དུས་འགྱངས།"

msgid "Time before session is considered idle"
msgstr "གླེང་མོལ་དེ་ཁོམ་ལོང་མ་བྱུང་གོང་གི་དུས་ཚོད་དུ་བརྩི་བ།"

msgid "Time in seconds before a click is triggered."
msgstr "ཆིག་རྡེབ་འཕྲད་འབྱུང་སྔོན་གྱི་སྐར་ཆའི་གྲངས། "

msgid "Time in seconds before a simulated secondary click is triggered."
msgstr "ཕལ་བའི་རྡེབ་གནོན་ལད་ཟློས་སྔོན་གྱི་སྐར་ཆའི་གྲངས། "

msgid "Timeout before click repeat"
msgstr "རྡེབ་གནོན་བསྐྱར་ཟློས་སྔོན་གྱི་དུས་འགྱངས།"

msgid "Timeout between click repeats"
msgstr "རྡེབ་གནོན་བསྐྱར་ཟློས་དབར་གྱི་དུས་འགྱངས།"

msgid ""
"Timeout in milliseconds before a click starts repeating (on spinner buttons "
"for example)."
msgstr ""
"རྡེབ་གནོན་ཐེངས་གཅིག་མགོ་བརྩམས་ནས་བསྐྱར་ཟློས་སྔོན་གྱི་དུས་འགྱངས་ཧའོ་སྐར་ཆའི་གྲངས།"
" (དཔེར་ན་ཕྲ་སྒྲིག་མཐེབ་རྡེབ་གནོན་སྐབས།)"

msgid ""
"Timeout in milliseconds between repeated clicks when a button is left "
"pressed."
msgstr ""
"གཡོན་མཐེབ་ཀྱིས་མཐེབ་གནོནག་གེ་མོར་ཆིག་རྡེབ་བྱ་སྐབས་བསྐྱར་ཟློས་རྡེབ་གནོན་དབར་གྱི་དུས་འགྱངས་ཧའོ་སྐར་ཆའི་གྲངས།"

msgid "Toggle fullscreen mode"
msgstr "བརྙན་ཡོལ་ཡོངས་འཆར་རྣམ་པར་བརྗེ་བ།"

msgid "Toggle maximization state"
msgstr "ཆེ་ཤོས་རྣམ་པར་བརྗེ་བ།"

msgid "Toggle shaded state"
msgstr "གྲིབ་ནག་རྣམ་པ་བརྗེ་བ།"

msgid "Toggle window always appearing on top"
msgstr "སྒེའུ་ཁུང་རྒྱུན་དུ་རྩེ་སྣེར་ཡོད་པར་བརྗེ་བ།"

msgid "Toggle window on all workspaces or one"
msgstr "ལས་དོན་ཁུལ་ཞིག་གམ་ཡོངས་ཀྱི་སྒེའུ་ཁུང་བརྗེ་བ།"

msgid "Toggle window to be always on top"
msgstr "སྒེའུ་ཁུང་རྒྱུན་དུ་རྩེ་སྣེར་ཡོད་པར་བརྗེ་བ།"

msgid "Toolbar Detachable"
msgstr "ལག་ཆའི་སྡེ་དབྲལ་ཆོག་པ།"

msgid "Toolbar Icon Size"
msgstr "ལག་ཆའི་སྡེ་གྱི་མཐེབ་གནོན་ཆེ་ཆུང་།"

msgid "Toolbar Style"
msgstr "ལག་ཆའི་སྡེ་ཡི་དཔེ་དབྱིབས།"

msgid "Toolbar Style. Valid values are \"both\", \"both-horiz\", \"icons\", and \"text\"."
msgstr ""
"ལག་ཆའི་སྡེ་གྱི་རྣམ་པ། ནུས་ལྡན་ཐང་ལ“both”、“both-"
"horiz”、“icon”དང་“text”བཅས་ཡོད།"

msgid "Touchpad button orientation"
msgstr "རེག་པང་མཐེབ་གནོན་གྱི་ཁ་ཕྱོགས།"

msgid "Touchpad enabled"
msgstr "རེག་པང་སྤྱོད་འགོ་རྩོམ།"

msgid "Touchscreen display mapping"
msgstr "འཕྲད་རེག་བརྙན་ཡོལ་རྟེན་འཕྲོ།"

msgid "Translate the name"
msgstr "མིང་འདི་ལོ་ཙཱ་གྱིས།"

msgid ""
"URI to use for the background image. Note that the backend only supports "
"local (file://) URIs."
msgstr ""
"རྒྱབ་ལྗོངས་བརྙན་རིས་ཀྱི་གནས་ས།（URI） "
"རྒྱབ་གནས་འདིས་གནས་དེ་གའི་URI（file:/）ཁོ་ན་ལས་རྒྱབ་སྐྱོར་མི་བྱེད། "

msgid "Unused; ignore"
msgstr "བེད་སྤྱོད་མ་བྱས་པ། སྣང་མེད་དུ་གཏོང་བ།"

msgid "Use different input sources for each window"
msgstr "སྒེའུ་ཁུང་རེ་རེར་ནང་འཇུག་གི་ཁུངས་མི་འདྲ་བ་སྤྱོད་དགོས།"

msgid "Use standard system font in window titles"
msgstr "སྒེའུ་ཁུང་གི་ཁ་བྱང་ནང་ཚད་ལྡན་གྱི་རྒྱུད་ཁོངས་ཡིག་གཟུགས་སྤྱོད་པ།"

msgid "User name to pass as authentication when doing HTTP proxying."
msgstr "HTTP ལས་ཚབ་ར་སྤྲོད་ལ་སྤྱོད་པའི་སྤྱོད་མཁན་གྱི་མིང་།"

msgid "Visual Bell Type"
msgstr "མིག་དབང་དྲིལ་བུའི་རིགས་གྲས།"

msgid ""
"When enabled, input sources get attached to the currently focused window "
"when activated."
msgstr ""
"བེད་སྤྱོད་གཏོང་སྐབས།ནང་འཇུག་ཁུངས་དེ་མིག་སྔའི་སྐུལ་སློང་བྱ་ཡུལ་གྱི་སྒེའུ་ཁུང་དང་འབྲེལ་བ་ཡོད།"
" "

msgid ""
"Whether animations should be displayed. Note: This is a global key, it "
"changes the behaviour of the window manager, the panel etc."
msgstr ""
"འགུལ་རིས་འཆར་རམ། "
"མཆན།འདི་ནི་ཁྱོན་ཡོངས་ཀྱི་མཐེབ་གནོན་ཞིག་ཡིན་ཞིང་།དེས་སྒེའུ་ཁུང་དོ་དམ་ཆས་དང་ངོས་པང་སོགས་ཀྱི་བྱ་སྤྱོད་བསྒྱུར་སྲིད།"

msgid "Whether buttons may display an icon in addition to the button text."
msgstr "མཐེབ་གནོན་ཡིག་ཆའི་འགྲམ་ནས་རིས་རྟགས་མངོན་ཐུབ་བམ། "

msgid "Whether menus may display an icon next to a menu entry."
msgstr "འདེམས་བྱང་ཚན་པའི་འགྲམ་དུ་རིས་རྟགས་མངོན་ཐུབ་བམ། "

msgid "Whether menus should have a tearoff."
msgstr "འདེམས་བྱང་དུ་མཚམས་ཐིག་འཆར་མིན།"

msgid ""
"Whether mnemonics should be automatically shown and hidden when the user "
"presses the Alt key."
msgstr ""
"སྤྱོད་མཁན་གྱིས་Altམཐེབ་གནོན་སྐབས་རང་འགུལ་ངང་འཛིན་གྲོགས་རྟགས་མངོན་པའམ་གབ་མིན།"

msgid ""
"Whether notification banners are visible for application notifications."
msgstr ""
"ཉེར་སྤྱོད་བྱ་རིམ་གྱི་བརྡ་ཐོའི་ངོས་ནས་བཤད་ན་བརྡ་ཐོའི་གཞུང་ཡིག་སྦྱར་ཚིག་མཐོང་ཐུབ་བམ།"
" "

msgid ""
"Whether notification banners for this application are shown or not. Does not"
" affect clicking on message tray buttons."
msgstr ""
"བྱ་རིམ་དེའི་བརྡ་ཐོའི་གཞུང་བཀལ་སྦྱར་ཚིག་མངོན་མིན། "
"ཐོ་སྡེར་གྱི་མཐེབ་གནོན་གནོན་པར་ཤུགས་རྐྱེན་མི་ཐེབས། "

msgid "Whether notifications are globally enabled for this application."
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་དེར་ཁྱོན་ཡོངས་ནས་བརྡ་ཐོ་སྤྱོད་འགོ་རྩོམ་མིན། "

msgid "Whether notifications are shown in the lock screen or not."
msgstr "བརྙན་ཡོལ་སྒྲོག་བཅིང་སྐབས་བརྡ་ཐོ་འཆར་མིན། "

msgid ""
"Whether notifications from this application are expanded automatically when "
"in banner mode."
msgstr ""
"གཞུང་བཀལ་སྦྱར་ཚིག་རྣམ་པའི་སྐབས་སུ་བྱ་རིམ་འདི་ནས་ཡོང་བའི་བརྡ་ཐོ་རང་འགྲེམ་བྱེད་མིན།"
" "

msgid ""
"Whether notifications from this application are shown when the screen is "
"locked."
msgstr "བརྙན་ཡོལ་དམིགས་འཛིན་སྐབས་བྱ་རིམ་དེ་ནས་ཡོང་བའི་བརྡ་ཐོ་འཆར་མིན། "

msgid "Whether notifications should be accompanied by sound alerts."
msgstr "བརྡ་ཐོར་ཉེན་བརྡའི་སྐད་ཀྱི་རམ་འདེགས་ཡོད་མེད། "

msgid "Whether the 'Bounce Keys' keyboard accessibility feature is turned on."
msgstr "མཐེབ་གཞོང་གི་གཞོགས་འདེགས་ནུས་པའི་ཁྲོད་ཀྱི་ལྡེམ་མཐེབ་ཕྱེ་ཡོད་མེད། "

msgid "Whether the 'Mouse Keys' accessibility feature is turned on."
msgstr "གཞོགས་འདེགས་བྱེད་ནུས་ཁྲོད་ཀྱི་ཙིག་མཐེབ་ཕྱེ་ཡོད་མེད། "

msgid "Whether the 'Slow Keys' accessibility feature is turned on."
msgstr "གཞོགས་འདེགས་བྱེད་ནུས་ཁྲོད་ཀྱི་དལ་མཐེབ་ཕྱེ་ཡོད་མེད། "

msgid "Whether the 'Toggle Keys' accessibility feature is turned on."
msgstr "གཞོགས་འདེགས་བྱེད་ནུས་ཁྲོད་ཀྱི་རྗེ་མཐེབ་ཕྱེ་ཡོད་མེད། "

msgid "Whether the clock displays in 24h or 12h format"
msgstr ""
"ཆུ་ཚོད་དེ་དུས་ཚོད་24ཡི་རྣམ་གཞག་གམ་དུས་ཚོད་12ཀྱི་རྣམ་གཞག་ཐོག་ནས་མངོན་པ།"

msgid "Whether the clock shows seconds"
msgstr "ཆུ་ཚོད་དུ་སྐར་ཆའི་གྲངས་འཆར་མིན།"

msgid ""
"Whether the context menus of entries and text views should offer to change "
"the input method."
msgstr ""
"ནང་འཇུག་ཚོད་འཛིན་དང་ཡིག་དེབ་མཐོང་རིས་ཁྲོད་ཀྱི་ཁོར་ཡུག་འདེམས་བྱང་གིས་འཇུག་ཐབས་བཟོ་བཅོས་བྱེད་པའི་འདེམ་བྱང་འདོན་སྤྲོད་བྱེད་མིན།"
" "

msgid ""
"Whether the context menus of entries and text views should offer to insert "
"control characters."
msgstr ""
"ནང་འཇུག་ཚོད་འཛིན་དང་ཡིག་དེབ་མཐོང་རིས་ཁྲོད་ཀྱི་ཁོར་ཡུག་འདེམས་བྱང་གིས་ཚོད་འཛིན་ཡིག་རྟགས་ཀྱི་འདེམ་བྱང་འདོན་སྤྲོད་བྱེད་མིན་།"
" "

msgid "Whether the cursor should blink."
msgstr "འོད་རྟགས་འོད་འཚེར་མིན། "

msgid "Whether the default tasks application needs a terminal to run."
msgstr ""
"སོར་བཞག་གི་ལས་འགན་གྱི་ཉེར་སྤྱོད་བྱ་རིམ་དེ་མཐའ་སྣེར་སླེབས་ན་ད་གཟོད་སྤྱོད་འགོ་རྩོམ་མིན།"
" "

msgid ""
"Whether the magnified view should be centered over the location of the "
"system mouse and move with it."
msgstr ""
"ཆེར་སྐྱེད་དཔེ་རིས་དེ་རྒྱུད་ཁོངས་ཀྱི་ཙིག་རྟགས་ཀྱི་གནས་བརྒྱུད་དེ་དཀྱིལ་སྒྲིག་བྱེད་མིན་དང་དེ་དང་བསྟུན་ནས་འགུལ་མིན།"
" "

msgid ""
"Whether the name key is a filename to be looked up in /usr/share/desktop-"
"directories."
msgstr ""
"མིང་མཐེབ་ནི་/usr/share/desktop-"
"directoriesཡི་ནང་དུ་ཡིག་ཆའི་མིང་འཚོལ་བཤེར་བྱེད་ཐུབ་པ་ཞིག་ཡིན་མིན། "

msgid "Whether the on-screen keyboard is turned on."
msgstr "བརྙན་ཡོལ་མཐེབ་གཞོང་སྤྱོད་འགོ་རྩོམ་མིན། "

msgid "Whether the screen magnifier is turned on."
msgstr "བརྙན་ཡོལ་སྐྱེད་ཤེལ་སྤྱོད་འགོ་རྩོམ་མིན། "

msgid "Whether the screen reader is turned on."
msgstr "བརྙན་ཡོལ་ཀྱི་ཀློག་འདོན་ཆས་སྤྱོད་འགོ་རྩོམ་མིན། "

msgid "Whether the sticky keys accessibility feature is turned on."
msgstr "གཞོགས་འདེགས་བྱེད་ནུས་ཁྲོད་ཀྱི་སྡུད་འབྱར་མཐེབ་ཕྱེ་ཡོད་དམ། "

msgid ""
"Whether the summary and body of notifications from this application will be "
"visible in the locked screen."
msgstr ""
"ཉེར་སྤྱོད་བྱ་རིམ་འདིའི་བརྡ་ཐོའི་གནད་བསྡུས་དང་ནང་དོན་དངོས་དེ་བརྙན་ཡོལ་སྟེང་མངོན་མིན།"
" "

msgid "Whether the user can detach menubars and move them around."
msgstr "སྤྱོད་མཁན་གྱིས་འདེམས་བྱང་སྡེ་ཁ་དབྲལ་བ་དང་གང་སར་སྤོ་ཆོག་གམ།"

msgid "Whether the user can detach toolbars and move them around."
msgstr "སྤྱོད་མཁན་གྱིས་ལག་ཆའི་སྡེ་ཁ་དབྲལ་བ་དང་གང་སར་སྤོ་ཆོག་གམ།"

msgid ""
"Whether the user can dynamically type a new accelerator when positioned over"
" an active menuitem."
msgstr ""
"སྤྱོད་མཁན་གྱིས་བྱ་འགུལ་གྱི་འདེམས་བྱང་ཚན་པར་དམིགས་འཛུགས་བྱེད་སྐབས་འགུལ་རྣམ་གྱིས་མྱུར་མཐེབ་གསར་བ་ནང་འཇུག་བྱེད་ཆོག་གམ།"

msgid "Whether the users full name is shown in the user menu or not."
msgstr "སྤྱོད་མཁན་གྱི་འདེམས་བྱང་དུ་སྤྱོད་མཁན་གྱི་མིང་ཆ་ཚང་མངོན་མིན། "

msgid "Whether to automatically mount media"
msgstr "བར་རྫས་རང་འགེལ་བྱེད་མིན།"

msgid "Whether to automatically open a folder for automounted media"
msgstr "རང་འགེལ་བྱེད་པའི་བར་རྫས་ལ་ཡིག་ཁུག་རང་འབྱེད་བྱེད་དགོས་མིན།"

msgid "Whether to automatically update the timezone using geolocation."
msgstr "རང་འགུལ་ངང་ས་བབ་ལ་གཞིགས་ནས་དུས་ཁུལ་གསར་སྒྱུར་བྱེད་མིན་། "

msgid "Whether to beep when a key is accepted."
msgstr "མཐེབ་གནོན་ལེན་སྐབས་སྦི་ཡི་སྒྲ་གྲགས་མིན། "

msgid "Whether to beep when a key is first pressed."
msgstr "ཐེངས་དང་པོར་མཐེབ་གང་རུང་མནན་ན་སྦི་གྲགས་མིན། "

msgid "Whether to beep when a key is rejected."
msgstr "མཐེབ་གང་ཞིག་དང་ལེན་མི་བྱེད་པའི་སྐབས་སུ་སྦི་སྒྲ་གྲགས་མིན། "

msgid ""
"Whether to beep when a keyboard accessibility feature is enabled or "
"disabled."
msgstr ""
"མཐེབ་གཞོང་གང་ཞིག་གི་གཞོགས་འདེགས་བྱེད་ནུས་སྤྱོད་འགོ་རྩོམ་པའི་མ་སྤྱོད་མི་ཆོག་པའི་སྐབས་སུ་སྦི་སྒྲ་གྲགས་མིན།"
" "

msgid "Whether to beep when a modifier key is pressed."
msgstr "རྒྱན་མཐེབ་གང་ཞིག་མནན་ན་སྦི་སྒྲ་གྲགས་མིན་། "

msgid ""
"Whether to disable keyboard accessibility after a timeout, useful for shared"
" machines."
msgstr ""
"དུས་ཚོད་ལས་བརྒལ་རྗེས་མཐེབ་གཞོང་གི་རམ་འདེགས་ནུས་པ་འགོག་མིན།རྩིས་འཁོར་མཉམ་སྤྱོད་སྐབས་ཕན་ཐོགས་ཡོད།"
" "

msgid ""
"Whether to disable sticky keys if two keys are pressed at the same time."
msgstr "མཐེབ་གནོན་གཉིས་མཉམ་དུ་མནན་དུས་སྡུད་འབྱར་མཐེབ་འགོག་མིན། "

msgid "Whether to play sounds on input events."
msgstr "ནང་འཇུག་གི་དོན་རྐྱེན་ཁྲོད་དུ་སྐད་གཏོང་ངམ། "

msgid "Whether to play sounds on user events."
msgstr "བེད་སྤྱོད་མཁན་གྱི་དོན་རྐྱེན་ཁྲོད་དུ་སྒྲ་གཏོང་ཐུབ་མིན། "

msgid "Whether to remember application usage"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་བེད་སྤྱོད་གནས་ཚུལ་ངེས་དགོས་མིན།"

msgid "Whether to remember recently used files"
msgstr "ཉེ་དུས་བེད་སྤྱོད་བྱས་པའི་ཡིག་ཆ་བློ་ལ་ངེས་མིན།"

msgid "Whether to remove old files from the trash automatically"
msgstr "རང་འགུལ་ངང་སྙིགས་སྣོད་ནང་གི་ཡིག་ཆ་རྙིང་པ་བསུབ་མིན།"

msgid "Whether to remove old temporary files automatically"
msgstr "རང་འགུལ་ངང་གནས་སྐབས་ཀྱི་ཡིག་ཆ་རྙིང་པ་བསུབ་མིན།"

msgid "Whether to resize with the right button"
msgstr "གཡས་མཐེབ་བཀོལ་ནས་ཆེ་ཆུང་བསྒྱུར་མིན།"

msgid "Whether toolkits should load accessibility related modules."
msgstr "ལག་ཆའི་སྒམ་ལ་རམ་འདེགས་ནུས་པ་དང་འབྲེལ་བའི་དཔེ་དུམ་སྣོན་འཇུག་དགོས་མིན། "

msgid "Whether windows should be raised when their client area is clicked"
msgstr "མཁོ་མཁན་ས་ཁོངས་ལ་རྡེབ་གནོན་སྐབས་Windowsམཐོ་རུ་གཏོང་དགོས་མིན་།"

msgid ""
"Width in pixels of the vertical and horizontal lines that make up the "
"crosshairs."
msgstr "རྒྱ་གྲམ་འོད་རྟགས་གྲུབ་བྱེད་ཀྱི་འཕྲེད་ཐིག་དང་གཞུང་ཐིག་གི་ཞེང་ཚད། "

msgid "Window focus mode"
msgstr "སྒེའུ་ཁུང་གི་མདོ་ཚེག་རྣམ་པ།"

msgid "Window scaling factor"
msgstr "སྒེའུ་ཁུང་སྐུམ་འཐེན་བཏགས་གྲངས།"

msgid "Window title font"
msgstr "སྒེའུ་ཁུང་ཁ་བྱང་ཡིག་གཟུགས།"
