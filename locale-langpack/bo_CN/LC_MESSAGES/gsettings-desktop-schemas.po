msgid "Action on title bar double-click"
msgstr "ཁ་བྱང་ཁག་ཉིས་རྡེབ་བྱེད་སྟངས།"

msgid "Action on title bar middle-click"
msgstr "དབུས་མཐེབ་ཀྱིས་ཁ་བྱང་སྡེ་ལ་རྡེབ་སྟངས།"

msgid "Action on title bar right-click"
msgstr "གཡས་མཐེབ་ཀྱིས་ཁ་བྱང་སྡེ་ལ་རྡེབ་སྟངས།"

msgid "Activate the window menu"
msgstr "སྒེའུ་ཁུང་འདེམས་བྱང་གྲུང་སློང་།"

msgid "Activate when idle"
msgstr "ཁོམ་ལོང་སྐབས་སྐུལ་སློང་བྱེད་པ།"

msgid "Allow embedding a keyboard into the window"
msgstr "མཐེབ་གཞོང་སྒེའུ་ཁུང་དུ་འཛུད་འཇུག་བྱས་ཆོག་པ།"

msgid "Allow logout"
msgstr "རྩིས་ཐེམ་ནས་བསུབ་ཆོག"

msgid "Allow the session status message to be displayed"
msgstr "གླེང་ལོལ་རྣམ་པའི་བརྡ་འཕྲིན་ཤར་ཆོག"

msgid "Allow user switching"
msgstr "སྤྱོད་མཁན་བརྗེ་ཆོག"

msgid "Always show the Universal Access status icon"
msgstr "རྒྱུན་དུ་རྒྱུན་སྤྱོད་ཀྱི་ལྟ་སྤྱོད་རྣམ་པའི་རིས་རྟགས་འཆར་བ།"

msgid "Application ID"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ།ID"

msgid "Applications"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ།"

msgid "Arrangement of buttons on the titlebar"
msgstr "ཁ་བྱང་རེའུ་མིག་མཐེབ་གནོན་བཀོད་པ།"

msgid "Authenticate proxy server connections"
msgstr "དཔང་ཡིག་ར་སྤྲོད་ལས་ཚབ་ཞབས་ཞུ་འཕྲུལ་ཆས་འབྲེལ་མཐུད།"

msgid "Automatic proxy configuration URL"
msgstr "རང་འགུལ་ལས་ཚབ་སྡེབ་སྒྲིག་URL"

msgid "Automatically raises the focused window"
msgstr "རང་འགུལ་མཐོར་འདེགས་ཀྱིས་མདོ་ཚེག་ཐོབ་པའི་སྒེའུ་ཁུང་།"

msgid "Automatically update timezone"
msgstr "དུས་ཁུལ་རང་འགུལ་གསར་སྒྱུར།"

msgid "Basename of the default keybinding theme used by gtk+."
msgstr ""
"gtk+ཡིས་སྤྱད་པའི་སོར་བཞག་མཐེབ་ཀྱིས་སྦྲེལ་བའི་བརྗོད་སྙིང་གི་རྨང་མིང་།（འགྲོ་ལམ་གྱི་ཡིག་ཆའི་མིང་ཚུད་མེད།）"
" "

msgid "Basename of the default theme used by gtk+."
msgstr "gtk+སྤྱད་པའི་བརྗོད་སྙིང་གི་རྨང་མིང་། "

msgid "Beep when a key is accepted"
msgstr "མཐེབ་གནོན་ནང་འཇུག་སྐབས་“སྦི”སྒྲ་གྲགས་པ་དང་ལེན།"

msgid "Beep when a key is first pressed"
msgstr "མཐེབ་གནོན་ཐེངས་དང་པོར་མར་མནན་དུས་“སྦི”་སྒྲ་གྲགས་པ།"

msgid "Beep when a key is rejected"
msgstr "མཐེབ་གནོན་དང་ལེན་མི་བྱེད་སྐབས་་“སྦི”་སྒྲ་གྲགས་པ།"

msgid "Beep when a keyboard accessibility feature changes"
msgstr "མཐེབ་གཞོང་རམ་འདེགས་བྱེད་ནུས་བཟོ་བཅོས་བྱེད་སྐབས་་“སྦི”་སྒྲ་གྲགས་པ།"

msgid "Beep when a modifier is pressed."
msgstr "རྒྱན་མཐེབ་གནོན་སྐབས་་“སྦི”་སྒྲ་གྲགས་པ།"

msgid "Binding to select the next input source"
msgstr "ནང་འཇུག་ཁུངས་རྗེས་མ་འདེམ་པ། མཐེབ་གནོན་མནན་ནས་སྦྲེལ་བ།"

msgid "Binding to select the previous input source"
msgstr "ནང་འཇུག་ཁུངས་གོང་མ་འདེམ་པ། མཐེབ་གནོན་མནན་ནས་སྦྲེལ་བ།"

msgid "Buttons Have Icons"
msgstr "མཐེབ་གནོན་ལ་རིས་རྟགས་ལྡན་པ།"

msgid "Calendar needs terminal"
msgstr "ལོ་རྩིས་ལ་མཐའ་སྣེ་དགོས།"

msgid "Can Change Accels"
msgstr "མྱུར་མཐེབ་བསྒྱུར་ཆོག"

msgid "Caret Tracking Mode"
msgstr "བར་བཅུག་རྗེས་འདེད་རྣམ་པ།"

msgid "Categories"
msgstr "རིགས་དབྱེ།"

msgid "Change brightness of blue"
msgstr "སྔོན་པོའི་གསལ་ཚད་བསྒྱུར་བ།"

msgid "Change brightness of green"
msgstr "ལྗང་ཁུའི་གསལ་ཚད་བསྒྱུར་བ།"

msgid "Change brightness of red"
msgstr "དམར་པོའི་གསལ་ཚད་བསྒྱུར་བ།"

msgid "Change contrast of blue"
msgstr "སྔོན་པོའི་བསྡུར་ཚད་བསྒྱུར་བ།"

msgid "Change contrast of green"
msgstr "ལྗང་ཁུའི་བསྡུར་ཚད་བསྒྱུར་བ།"

msgid "Change contrast of red"
msgstr "དམར་པོའི་བསྡུར་ཚད་བསྒྱུར་བ།"

msgid "Click method"
msgstr "གནོན་ཐབས།"

msgid "Clip the crosshairs at the center"
msgstr "ལྟེ་གནས་ནས་རྒྱ་གྲམ་གྱི་འོད་རྟགས་དྲས་རྒྱག"

msgid "Close window"
msgstr " སྒེའུ་ཁུང་ཁ་རྒྱག་པ།"

msgid "Color Saturation"
msgstr "ཁ་དོག་འདང་ཚད།"

msgid "Color Shading Type"
msgstr "མདོག་རིམ་འགྱུར་གྱི་རིགས་གྲས།"

msgid "Color of the crosshairs"
msgstr "རྒྱ་གྲམ་འོད་རྟགས་ཀྱི་ཁ་དོག"

msgid "Consider trash and temporary files old after this many days."
msgstr ""
"ཉིན་འདི་དག་ལས་བརྒལ་བའི་སྙིགས་སྣོད་དང་གནས་སྐབས་ཡིག་ཆ་དག་ཡིག་ཆ་རྙིང་པར་བརྩི་བ།"
" "

msgid "Control how new windows get focus"
msgstr ""
"སྒེའུ་ཁུང་གསར་པས་ཇི་ལྟར་བྱས་ནས་མདོ་ཚེག་ཐོབ་ལེན་བྱེད་པ་ཚོད་འཛིན་བྱེད་དགོས།"

msgid "Controls visibility of personal information"
msgstr "མི་སྒེར་གྱི་ཆ་འཕྲིན་མཐོང་ཐུབ་རང་བཞིན་ཚོད་འཛིན་པ།"

msgid "Current input source"
msgstr "མིག་སྔའི་ནང་འཇུག་ཁུངས།"

msgid "Current theme"
msgstr "མིག་སྔའི་བརྗོད་གཞི།"

msgid "Cursor Blink"
msgstr "འོད་རྟགས་འོད་འཚེར།"

msgid "Cursor Blink Time"
msgstr "འོད་རྟགས་འཚེར་བའི་དུས་ཚོད།"

msgid "Cursor Blink Timeout"
msgstr "འོད་རྟགས་འོད་འཚེར་དུས་བརྒལ།"

msgid "Cursor size"
msgstr "འོད་རྟགས་ཀྱི་ཆེ་ཆུང་།"

msgid "Cursor theme"
msgstr "འོད་རྟགས་ཀྱི་བརྗོད་གཞི།"

msgid ""
"Cursor theme name. Used only by Xservers that support the Xcursor extension."
msgstr ""
"འོད་རྟགས་ཀྱི་བརྗོད་གཞིའིའི་མིང་། "
"Xcursorརྒྱ་སྐྱེད་ལ་རྒྱབ་སྐྱོར་བྱེད་པའི་Xserverཁོ་ནས་སྤྱད་ཆོག "

msgid "Default calendar"
msgstr "སོར་བཞག་ལོ་ཐོ།"

msgid "Default font"
msgstr "སོར་བཞག་ཡིག་གཟུགས།"

msgid "Default tasks"
msgstr "སོར་བཞག་ལས་འགན།"

msgid "Default tasks application."
msgstr "སོར་བཞག་ལས་འགན་ཉེར་སྤྱོད་བྱ་རིམ།"

msgid "Defines the situations in which the touchpad is enabled."
msgstr "རེག་པང་ལ་མཚན་ཉིད་བཞག་ནས་སྤྱོད་འགོ་རྩོམ་པའི་གནས་ཚུལ། "

msgid "Delay between repeats in milliseconds."
msgstr "བསྐྱར་ཟློས་བར་གྱི་དུས་འགྱངས། (ཧའོ་སྐར་ཆ།)"

msgid "Delay in milliseconds for the auto raise option"
msgstr "སྒེའུ་ཁུང་རང་འགུལ་ངང་མཐོ་རུ་འགྲོ་བའི་དུས་འགྱངས།(ཧའོ་སྐར་ཆ།)"

msgid ""
"Determines the length in pixels of the vertical and horizontal lines that "
"make up the crosshairs."
msgstr ""
"རྒྱ་གྲམ་འོད་རྟགས་ཀྱི་ཆུ་སྙོམས་ཐིག་དང་དྲང་ཐིག་གི་རིང་ཚད་གྲུབ་སྟངས་གཏན་འཁེལ། "
"(བརྙན་རྒྱུས་བརྩི་བ།)"

msgid ""
"Determines the transparency of the crosshairs, from fully opaque to fully "
"transparent."
msgstr ""
"འོད་རྟགས་ཀྱི་གསལ་ཚད་གནཏ་འཁེལ། རྩ་བ་ནས་མི་གསལ་པ་ནས་ཡོངས་སུ་དྭངས་གསལ་གྱི་བར། "

msgid ""
"Determines whether the lightness values are inverted: darker colors become "
"lighter and vice versa, and white and black are interchanged."
msgstr ""
"གསལ་ཚད་ཀྱི་ཐང་དེ་གོ་ལྡོག་མིན་སྒྲིག་གཅོད་པ།ཅུང་ནག་པའི་ཁ་དོག་དེ་ཅུང་གསལ་བར་འགྱུར་བ།ཅིག་ཤོས་དེ་ལས་ལྡོག་པ་སྟེ།"
" དཀར་ནག་བརྗེ་བ་ཐེབ་པ།"

msgid "Direction to perform a double click ('left', 'right', 'up', 'down')."
msgstr "ཉིས་རྡེག་ལག་བསྟར་བྱེད་པའི་ཁ་ཕྱོགས།(“left”、“right”、“up”、“down”)"

msgid ""
"Direction to perform a secondary click ('left', 'right', 'up', 'down')."
msgstr "རྡེབ་གནོན་ཕལ་བ་ལག་བསྟར་（གཡས་མཐེབ་རྡེབ་གནོན་）གྱི་ཁ་ཕྱོགས། "

msgid "Direction to perform a single click ('left', 'right', 'up', 'down')."
msgstr "རྐྱང་རྡེབ་ལག་བསྟར་གྱི་ཁ་ཕྱོགས(“left”、“right”、“up”、“down”)。"

msgid "Direction to perform dragging ('left', 'right', 'up', 'down')."
msgstr "འདྲུད་སྒུལ་ལག་བསྟར་གྱི་ཁ་ཕྱོགས།(“left”、“right”、“up”、“down”)"

msgid "Disable URL and MIME type handlers"
msgstr " URL དང་ MIME རིགས་ཀྱི་སྒྲིག་གཅོད་ཆས་སྤྱོད་མི་ཆོག"

msgid "Disable all external search providers"
msgstr "ཕྱིའི་འཚོལ་བཤེར་བྱ་རིམ་ཡོད་ཚད་བཀོལ་མི་ཆོག"

msgid "Disable all external thumbnailers"
msgstr "ཕྱི་རོལ་གྱི་བསྡུས་རིས་བྱ་རིམ་ཡོད་ཚད་སྤྱོད་མི་ཆོག་པ།"

msgid "Disable command line"
msgstr "་བཀའ་བརྡའི་ཕྲེང་སྤྱོད་མི་ཆོག་པ།"

msgid "Disable keyboard accessibility after a timeout"
msgstr "དུས་བརྒལ་རྗེས་མཐེབ་གཞོང་རམ་འདེགས་ནུས་པ་སྤྱོད་མི་ཆོག"

msgid "Disable lock screen"
msgstr "བརྙན་ཡོལ་དམིགས་འཛིན་སྤྱད་མི་ཆོག"

msgid "Disable log out"
msgstr "རྩིས་ཐེམ་ནས་སུབ་པ་སྤྱོད་མི་ཆོག"

msgid "Disable misfeatures that are required by old or broken applications"
msgstr ""
