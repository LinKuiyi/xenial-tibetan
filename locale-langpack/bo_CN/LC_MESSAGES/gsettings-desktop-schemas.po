msgid "If true, applications are allowed to access location information."
msgstr ""
"གལ་ཏེ་བདེན་ན། ཉེར་སྤྱོད་བྱ་རིམ་དེ་གནས་གཞིའི་ཆ་འཕྲིན་ལ་བལྟ་རུ་འཇུག་ངེས། "

msgid "If true, display date in the clock, in addition to time."
msgstr "གལ་ཏེ་ trueཡིན་ན།དུས་ཚོད་ཕུད་ཆུ་ཚོད་ལའང་ཟླ་ཚེ་འཆར། "

msgid "If true, display seconds in the clock."
msgstr "གལ་ཏེ་ trueཡིན་ན།ཆུ་ཚོད་ཀྱི་གནས་སུ་སྐར་ཆ་ཡི་གྲངས་ཀ་འཆར། "

msgid ""
"If true, ignore the titlebar-font option, and use the standard application "
"font for window titles."
msgstr ""
"གལ་ཏེ་trueཡིན་ན།ཁ་བྱང་སྡེ་ཡི་ཡིག་གཟུགས་འདེམ་བྱང་སྣང་མེད་གཏོང་བ་མ་ཟད།སྒེའུ་ཁུང་གི་ཁ་བྱང་ལ་ཚད་ལྡན་ཉེར་སྤྱོད་ཡིག་གཟུགས་བཀོལ།"
" "

msgid "Ignore multiple presses of the same key within this many milliseconds."
msgstr ""
"ཧའོ་སྐར་ཆ་གཅིག་གི་ནང་དུ་མཐེབ་གནོན་གཅིག་འདྲ་ཐེངས་མང་མནན་པར་སྣང་མེད་བྱེད། "

msgid "Initial Key Repeat Delay"
msgstr "ཐོག་མའི་མཐེབ་གནོན་བསྐྱར་ཟློས།"

msgid "Initial delay in milliseconds"
msgstr "ཐོག་མའི་ཕྱིར་འགྱངས།(ཧའོ་སྐར་ཆ།)"

msgid "Initial key repeat delay in milliseconds."
msgstr "ཐོག་མའི་མཐེབ་གནོན་བསྐྱར་ཟློས་ཕྱིར་འགྱངས།(ཧའོ་སྐར་ཆ།)"

msgid "Input feedback sounds"
msgstr "ཚུར་ལན་སྐད་འཇུག"

msgid ""
"Integer factor used to scale windows by. For use on high-dpi screens. 0 "
"means pick automatically based on monitor."
msgstr ""
"སྒེའུ་ཁུང་གི་སྐྱེད་སྐྱུང་ལ་སྤྱོད་པའི་ཧྲིལ་གྲངས་ཞུགས་གྲངས། "
"དབྱེ་འབྱེད་ཚད་མཐོ་བའི་བརྙན་ཡོལ་ལ་སྤྱོད། "
"0ཡིས་འཆར་ཆས་ལ་གཞིགས་ནས་རང་འདེམས་བྱེད་པ་མཚོན། "

msgid "Inverse lightness"
msgstr "ལྡོག་འཁོར་གསལ་ཚད།"

msgid "Key Repeat Interval"
msgstr "མཐེབ་གནོན་བསྐྱར་ཟློས་བར་ཆོད།"

msgid "Keyboard repeat"
msgstr "མཐེབ་གཞོང་བསྐྱར་ཟློས།"

msgid "Keyboard shortcut to open the menu bars."
msgstr "འདེམས་བྱང་སྡེའི་མཐེབ་གཞོང་མྱུར་མཐེབ་ཕྱེ་བ།"

msgid "Left or Top color when drawing gradients, or the solid color."
msgstr ""
"རིམ་འགྱུར།དེ་ནི་གཡོན་ངོས་སམ་ཡང་ན་སྟེང་ངོས་ཀྱི་ཁ་དོག་དང་།ཡང་ན་མདོག་གཙང་མ།"

msgid "Length of the crosshairs in pixels"
msgstr "རྒྱ་གྲམ་འོད་རྟགས་ཀྱི་རིང་ཚད།"

msgid "Length of the cursor blink cycle, in milliseconds."
msgstr "འོད་རྟགས་འོད་འཚེར་བའི་འཁོར་ཡུན་རིང་ལ།སྡེ་ཚན་ནི་སྐར་ཆ་གཅིག་ཡིན།"

msgid "List of XKB options"
msgstr "XKBའདེམས་བྱང་རེའུ་མིག"

msgid "List of desktop file IDs for search providers sort order"
msgstr "བཤེར་འཚོལ་བྱ་རིམ་རིམ་སྒྲིག་ཏུ་བཀོལ་བའི་ཅོག་ངོས་ཡིག་ཆIDརེའུ་མིག"

msgid ""
"List of desktop file IDs for which the associated default-disabled search "
"provider should be enabled"
msgstr ""
"སོར་བཞག་ཁ་འབྱེད་ལ་མཁོ་བའི་འབྲེལ་ཡོད་བཤེར་འཚོལ་ཞབས་ཞུའི་ནང་ཚུད་པའི་ཅོག་ངོས་ཡིག་ཆIDརེའུ་མིག"

msgid ""
"List of desktop file IDs for which the associated default-enabled search "
"provider should be disabled"
msgstr ""
"སོར་བཞག་ཁ་འབྱེད་ལ་མཁོ་བའི་འབྲེལ་ཡོད་བཤེར་འཚོལ་ཞབས་ཞུས་ཕར་ཕུད་པའི་ཅོག་ངོས་ཡིག་ཆIDརེའུ་མིག"

msgid "List of input sources"
msgstr "ནང་འཇུག་ཁུངས་ཀྱི་རེའུ་མིག"

msgid ""
"List of mime-types for which external thumbnailer programs will be disabled"
msgstr "ཕྱིའི་བསྡུས་རིས་བྱ་རིམ་ཀྱི་mime རིགས་གྲས་རེའུ་མིག་སྤྱད་མི་ཆོག་པ།"

msgid "List of symbolic names and color equivalents"
msgstr "མཚོན་རྟགས་མིང་དང་ཁ་དོག་ཚབ་ཨང་གི་ལྟོས་ཟླའི་རེའུ་མིག"

msgid "List of x-content/* types set to \"Do Nothing\""
msgstr "“ལག་བསྟར་མི་བྱེད་པའི་བཀོལ་སྤྱོད་”ཀྱི་x-content/* རིགས་གྲས་རེའུ་མིག"

msgid "List of x-content/* types set to \"Open Folder\""
msgstr "“ཡིག་ཁུག་ཁ་འབྱེད”ལ་བཀོད་པའི་ x-content/* རིགས་གྲས་རེའུ་མིག"

msgid ""
"List of x-content/* types where the preferred application will be launched"
msgstr ""
"འདྲེན་སྤྱོད་ཐོག་འདེམས་ཉེར་སྤྱོད་བྱ་རིམ་གྱི་ x-content/* རིགས་གྲས་རེའུ་མིག"

msgid "Lock on activation"
msgstr "སྐུལ་སློང་སྐབས་དམིགས་འཛིན།"

msgid "Lock on suspend"
msgstr "ངང་སྒུག་སྐབས་དམིགས་འཛིན།"

msgid "Logout command"
msgstr "རྩིས་ཐེམ་ནས་བསུབ་པའི་བཀའ་བརྡ།"

msgid "Magnification factor"
msgstr "ཆེར་སྐྱེད་ལྡབ་གྲངས།"

msgid ""
"Makes all installed input sources available for choosing in System Settings."
msgstr ""
"སྒྲིག་འཇུག་བྱས་ཟིན་པའི་ནང་འཇུག་ཁུངས་ཚང་མ་རྒྱུད་ཁོངས་སྒྲིག་འགོད་ཁྲོད་སྒྲིག་ནས་གདམ་ག་བྱེད་རྒྱུར་མཁོ་འདོན་བྱེད་པ།"
" "

msgid "Maximize window"
msgstr "སྒེའུ་ཁུང་ཆེས་ཆེར་སྐྱེད།"

msgid "Maximize window horizontally"
msgstr "སྒེའུ་ཁུང་ཆུ་སྙོམས་ཆེས་ཆེར་བསྐྱེད།"

msgid "Maximize window vertically"
msgstr "སྒེའུ་ཁུང་དྲང་དཕྱང་ཆེས་ཆེར་བསྐྱེད།"

msgid ""
"Maximum age for thumbnails in the cache, in days. Set to -1 to disable "
"cleaning."
msgstr ""
"འགྱངས་གསོག་ཁྲོད་བསྡུས་རིས་གསོག་ཉར་ཚགས་ཀྱི་ཉིན་གྲངས། -1 "
"ལ་སྒྲིག་ནས་དུས་ཡོལ་གཙང་སེལ་བྱེད་པ་བཀག་འགོག་བྱེད་པ།"

msgid ""
"Maximum size of the thumbnail cache, in megabytes. Set to -1 to disable "
"cleaning."
msgstr ""
"རི་མོ་བསྡུས་བཅོས།ལྷོད་གསོག་གི་ཆེས་ཆེ་བའི་ཚད་གཞི་ནིMBཡིན། "
"(1）ཚད་བརྒལ་གཙང་བཤེར་བྱེད་པར་གཏན་འགོག་བྱ་དགོས།"

msgid "Menubar Detachable"
msgstr "འདེམས་བྱང་སྡེཁ་བྲལ་ཆོག་པ།"

msgid "Menubar accelerator"
msgstr "འདེམས་བྱང་སྡེའི་མྱུར་མཐེབ།"

msgid "Menus Have Icons"
msgstr "འདེམས་བྱང་ལ་རིས་རྟགས་ལྡན་པ།"

msgid "Menus Have Tearoff"
msgstr "འདེམས་བྱང་ལ་དབྱེ་ཐིག་ལྡན་པ།"

msgid "Minimize window"
msgstr "སྒེའུ་ཁུང་ཆུང་སྒྱུར།"

msgid "Minimum interval in milliseconds"
msgstr "ཆེས་ཆུང་བར་ཐག(ཧའོ་སྐར་ཆ།)"

msgid "Modifier to use for modified window click actions"
msgstr "སྒེའུ་ཁུང་ལ་གནོན་བྱེད་ཀྱི་རྒྱན་མཐེན།"

msgid "Monospace font"
msgstr "ཞེང་མཚུངས་ཡིག་གཟུགས།"

msgid "Mouse Tracking Mode"
msgstr "ཙིག་རྟགས་རྗེས་འདེད་རྣམ་པ།"

msgid "Mouse button orientation"
msgstr "ཙིག་རྟགས་མཐེབ་གནོན་ཁ་ཕྱོགས།"

msgid "Mouse wheel emulation button. 0 to disable the feature."
msgstr "ཙིག་རྟགས་འགྲིལ་འཁོར་ལད་ཟློས་མཐེབ་གནོན་། 0ཡིས་ཕན་ནུས་འདི་བཀག་པ་མཚོན།"

msgid "Move window"
msgstr "སྒེའུ་ཁུང་སྤོར་བ།"

msgid "Move window one workspace down"
msgstr "སྒེའུ་ཁུང་དེ་འཆར་ཆས་གཅིག་གིས་ཡར་སྤོས།"

msgid "Move window one workspace to the left"
msgstr "སྒེའུ་ཁུང་གཡོན་དུ་ལས་དོན་ཁུལ་ཞིག་ཏུ་བསྒྱུར་དགོས།"

msgid "Move window one workspace to the right"
msgstr "སྒེའུ་ཁུང་གཡས་ལ་ལས་དོན་ཁུལ་གཅིག་སྤོ་རྒྱུ་ཡིན་པ།"

msgid "Move window one workspace up"
msgstr "སྒེའུ་ཁུང་ཐོག་ལས་དོན་ཁུལ་ཞིག་ཏུ་བསྒྱུར་བ།"

msgid "Move window to bottom edge of screen"
msgstr "སྒེའུ་ཁུང་བརྙན་ཡོལ་གྱི་མཐིལ་སྣེ་རུ་སྤོས།"

msgid "Move window to bottom left corner"
msgstr "སྒེའུ་ཁུང་གཡོན་ཟུར་དུ་སྤོས།"

msgid "Move window to bottom right corner"
msgstr "སྒེའུ་ཁུང་གཡས་ཀྱི་འོག་ཟུར་དུ་སྤོས།"

msgid "Move window to center of screen"
msgstr "སྒེའུ་ཁུང་བརྙན་ཡོལ་གྱི་དཀྱིལ་དུ་སྤོས།"

msgid "Move window to last workspace"
msgstr "སྒེའུ་ཁུང་ནས་མདུན་གྱི་ལས་ཀའི་བར་སྟོང་དུ་སྤོས།"

msgid "Move window to left side of screen"
msgstr "སྒེའུ་ཁུང་དེ་བརྙན་ཡོལ་གྱི་གཡོན་ཕྱོགས་སུ་སྤོས།"

msgid "Move window to right side of screen"
msgstr "སྒེའུ་ཁུང་བརྙན་ཡོལ་གྱི་གཡས་འགྲམ་དུ་སྤོ་པ།"

msgid "Move window to the next monitor above"
msgstr "སྒེའུ་ཁུང་དེ་སྟེང་གི་འཆར་ཆས་སྟེང་དུ་སྤོས།"

msgid "Move window to the next monitor below"
msgstr "སྒེའུ་ཁུང་དེ་གཤམ་འོག་གི་འཆར་ཆས་སྟེང་དུ་སྤོ་དགོས།"

msgid "Move window to the next monitor on the left"
msgstr "སྒེའུ་ཁུང་དེ་གཡོན་ཕྱོགས་ཀྱི་འཆར་ཆས་སྟེང་དུ་སྤོས།"

msgid "Move window to the next monitor on the right"
msgstr "སྒེའུ་ཁུང་གཡས་ཕྱོགས་ཀྱི་འཆར་ཆས་སྟེང་དུ་སྤོས།"

msgid "Move window to top edge of screen"
msgstr "སྒེའུ་ཁུང་དེ་བརྙན་ཡོལ་རྩེ་གནས་ཀྱི་མཐའ་རུ་སྤོས།"

