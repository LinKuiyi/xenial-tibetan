msgid "Authentication is needed to diagnose graphics issues"
msgstr "པར་རིས་གནད་དོན་ལ་བརྟག་དཔྱད་བྱེད་པར་བདེན་དཔང་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Debug"
msgstr " ཚོད་སྒྲིག"

msgid "Diagnose Graphics Issues"
msgstr "རིས་དབྱིབས་བརྟག་དཔྱད་ཀྱི་གནད་དོན།"

msgid "Disable _PAT memory"
msgstr "PATནང་གསོག་སྤྱོད་མི་ཆོག"

msgid "Disable _VESA framebuffer driver"
msgstr " -VESAལྡེབ་ལྷོད་གཏོང་སྐུལ་འདེད་སྤྱོད་མི་ཆོག"

msgid "Disable bootloader _graphics"
msgstr "རིས་དབྱིབས་ཅན་གྱི་སྣེ་ཁྲིད་བྱ་རིམ་སྤྱོད་མི་ཆོག(_G)"

msgid "Display boot messages"
msgstr " འགོ་སློང་ཆ་འཕྲིན་འཆར་བ།"

msgid "Enable automatic crash bug reporting"
msgstr " ཐོར་ཞིག་སྐབས་རང་འགུལ་ངང་ནོར་འཁྲུལ་ཆ་འཕྲིན་ཡར་ཞུ་བྱེད་པ།"

msgid "Error Message"
msgstr " ནོར་འཁྲུལ་བརྡ་འཕྲིན།"

msgid "Extra graphics _debug messages"
msgstr " དེ་ལས་མང་བའི་བརྙན་རིས་ཚོད་སྒྲིག་ཆ་འཕྲིན།"

msgid "List dates on which updates were performed"
msgstr " ལག་བསྟར་གསར་བསྒྱུར་གྱི་དུས་ཚོད་འཆར་བ།"

msgid ""
"Makes dmesg logs more verbose with details about 3d, plymouth, and monitor "
"detection"
msgstr ""
"  dmesg ལ་ "
"3Dདང་plymouthདང་འཆར་ཆས་བཤེར་འཇལ་འབྲེལ་ཡོད་ཆ་འཕྲིན་སོགས་ཟིན་ཐོ་ཞིབ་འགོད་བྱེད་དུ་འཇུག་པ།"

msgid "Only include entries from this date and earlier (YYYY/MM/DD)"
msgstr "ཚེས་གྲངས་དང་དེའི་སྔོན་གྱི་ཟིན་ཐོ་མ་གཏོགས་ཚུད་མེད།"

msgid "Only include entries from this date forward (YYYY/MM/DD)"
msgstr "ཚེས་གྲངས་དེའི་རྗེས་ཀྱི་ཟིན་ཐོ་མ་གཏོགས་ཚུད་མེད།"

msgid "Report an Xorg Bug"
msgstr " Xorg ནོར་འཁྲུལ་སྙན་ཞུ།"

msgid "Troubleshoot failure to start a graphical X11 session"
msgstr " X11 གླེང་མོལ་འགོ་སློང་ཚོད་སྒྲིག་སྐབས་བྱུང་བའི་སྐྱོན།"

msgid "Turns on the Apport crash detection and bug reporting tool"
msgstr ""
"Apportཐོར་ཞིག་ཞིབ་དཔྱད་ཚོད་ལེན་དང་ནོར་འཁྲུལ་སྙན་ཞུའི་ཡོ་བྱད་འགོ་སློང་།"

msgid "View Errors"
msgstr "ནོར་འཁྲུལ་ལ་ལྟ་ཞིབ་བྱེད་པ།"

msgid "Workarounds"
msgstr "ལས་ཁུལ།"

msgid "X Diagnostics Settings"
msgstr " X བརྟག་དཔྱད་ཡོ་བྱད་སྒྲིག་འགོད།"

msgid "Xorg Error Messages"
msgstr " Xorg ནོར་འཁྲུལ་ཆ་འཕྲིན།"
