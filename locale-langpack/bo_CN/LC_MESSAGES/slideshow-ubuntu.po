msgid "Appearance"
msgstr "ཕྱི་ཚུལ།"

msgid "Assistive technologies"
msgstr "རམ་འདེགས་ལག་རྩལ།"

msgid "Customization options"
msgstr "མཚན་ཉིད་རང་འཇོག་འདེམས་ཚན།"

msgid "Everything you need for the office"
msgstr "གཞུང་ལས་སུ་མཁོ་བ་ཆ་ཚང་འདུས་ཡོད།"

msgid "Find even more software"
msgstr "མཉེན་གང་མང་སྟབས་བདེ་ངང་ཐོབ་ཐུབ།"

msgid "Firefox web browser"
msgstr "Firefoxདྲ་ངོས་བཤར་ཆས།"

msgid "Flash"
msgstr "འོད་འཁྱུག་གློག་སྒྲོན།"

msgid "GIMP Image Editor"
msgstr "GIMP བརྙན་རིས་རྩོམ་སྒྲིག་ཆས།"

msgid "Have fun with your photos"
msgstr "གཟུགས་བརྙན་སྒྲིག་གཅོད། སྤྲོ་སྣང་རབ་རྒྱས།"

msgid "Help and support"
msgstr "རོགས་རམ་དང་རྒྱབ་སྐྱོར།"

msgid "Included software"
msgstr "མཉེན་ཆས་སྔོན་འཇུག"

msgid "Language support"
msgstr "སྐད་བརྡ་རྒྱབ་སྐྱོར།"

msgid "Make the most of the web"
msgstr "དྲ་ངོས་ལྟ་བཤར། ཐགས་ཐོགས་མེད་པ།"

msgid "Pitivi Video Editor"
msgstr "Pitivi བརྙན་ཟློས་རྩོམ་སྒྲིག་ཆས།"

msgid "Rhythmbox Music Player"
msgstr "Rhythmboxརོལ་དབྱངས་གཏོང་ཆས།"

msgid "Shotwell Photo Manager"
msgstr "Shotwlelའདྲ་པར་དོ་དམ་ཆས།"

msgid "Supported software"
msgstr "རྒྱབ་སྐྱོར་བྱེད་པའི་མཉེན་ཆས།"

msgid "Take your music with you"
msgstr "རོལ་དབྱངས་སྙན་མོ། གཏག་ཏུ་མཉམ་སྐྱོད།"

msgid "Welcome to Ubuntu"
msgstr "Ubuntuབེད་སྤྱོད་པར་དགའ་བསུ་ཞུ།"
