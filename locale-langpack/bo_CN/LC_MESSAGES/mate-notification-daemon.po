msgid "Use Active Monitor"
msgstr "བཀོལ་སྤྱོད་བྱེད་སྒོའི་མངོན་ཤེལ་ཡོ་བྱད།"

msgid "_Monitor:"
msgstr "འཆར་ཆས།(_M)："

msgid "_Preview"
msgstr "སྔོན་ལྟ། (_P)"

msgid "_Theme:"
msgstr "ཁ་བྱང་། (_T)："
