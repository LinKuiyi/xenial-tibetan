msgid "%s is not a LUKS partition\n"
msgstr "%s ནི་ LUKS ཁུལ་བགོད་ཅིག་མིན།\n"

msgid "%s: requires %s as arguments"
msgstr "%s: requires %s ཞུགས་གྲངས་བྱ།"

msgid "<device>"
msgstr "<སྒྲིག་ཆས།>"

msgid "<device> <key slot>"
msgstr "<སྒྲིག་ཆས།> <key slot>"

msgid "<device> <name> "
msgstr "<སྒྲིག་ཆས།> <མིང་> "

msgid "<device> [<new key file>]"
msgstr "<སྒྲིག་ཆས།> [<གསང་ལྡེ་གསར་བའི་ཡིག་ཆ།>]"

msgid "<name>"
msgstr "<མིང་>"

msgid "<name> <device>"
msgstr "<མིང་།> <སྒྲིག་ཆས།>"

msgid "Align payload at <n> sector boundaries - for luksFormat"
msgstr "luksFormat ལ་འགྲེམས་པའི་ནུས་ལྡན་ཁུར་བོ<n>གཡབ་ཁུལ་མཐའ་མཚམས་སུ་གནས།"

msgid "Argument <action> missing."
msgstr "<action>ཞུགས་གྲངས་ཆད་པ།"

msgid "BITS"
msgstr "གནས་གྲངས།"

msgid "Can't open device: %s\n"
msgstr "སྒྲིག་ཆས།： %s ཁ་འབྱེད་མི་ཐུབ།\n"

msgid "Command failed"
msgstr "བཀའ་བརྡ་ཕམ་པ།"

msgid "Command successful.\n"
msgstr "བཀའ་བརྡ་ལེགས་འགྲུབ།\n"

msgid "Create a readonly mapping"
msgstr "ཀློག་ཙམ་གྱི་རྟེན་འཕྲོ་ཞིག་གསར་བཟོ།"

msgid "Display brief usage"
msgstr "སླ་བསྡུས་ཀྱི་སྤྱོད་སྟངས་མངོན་པ།"

msgid "Do not ask for confirmation"
msgstr "མི་འདྲི་བ་གཏན་འཁེལ།"

msgid "Failed to obtain device mapper directory."
msgstr "སྒྲིག་ཆས་རྟེན་འཕྲོའི་དཀར་ཆག་ཐོབ་པ་ཕམ་པ།"

msgid "Help options:"
msgstr "རོགས་རམ་འདེམས་ཚན།:"

msgid "How many sectors of the encrypted data to skip at the beginning"
msgstr "གསང་བསྡོམས་གཞི་གྲངས་གཡབ་ཁུལ་དུ་མཆོང་བཞིན་ཡོད།"

msgid "How often the input of the passphrase can be retried"
msgstr "དུས་ཡུན་དུ་རུ་བསྐྱར་ཚོད་བྱ་ནས་གསང་ལྡེ་ནང་འཇུག་ཐུབ།"

msgid "Key %d not active. Can't wipe.\n"
msgstr "གསང་ལྡེ་ %d སྐུལ་སློང་བྱས་མེད།  བསུབ་མི་ཉན།\n"

msgid "Key size must be a multiple of 8 bits"
msgstr "གསང་ཨང་གི་རིང་ཚད་ངེས་པར་དུ་8 ཡི་ལྡབ་གྲངས་ཡིན་དགོས།"

msgid "PBKDF2 iteration time for LUKS (in ms)"
msgstr "LUKS ཡི་PBKDF2 ཡང་བསྐྱར་བྱེད་པའི་དུས་ཚོད། ( ཧའོ་སྐར་ཆའི་ནང་།)"

msgid "Print package version"
msgstr "ཐུམ་པར་གཞི་མངོན་པ།"

msgid "Read the key from a file (can be /dev/random)"
msgstr "ཡིག་ཆ་ཞིག་ཏུ་གསང་ཨང་ཀློག་པ།( /dev/random རུ་ཡོད་སྲིད།)"

msgid "SECTORS"
msgstr "གཡབ་ཁུལ་གྲངས།"

msgid "Show this help message"
msgstr "རོགས་རམ་འདིའི་ཆ་འཕྲིན་མངོན་པ།"

msgid "Shows more detailed error messages"
msgstr "ཞིབ་ཕྲའི་ནོར་འཁྲུལ་ཆ་འཕྲིན་ཇེ་མང་མངོན་པ།"

msgid "The cipher used to encrypt the disk (see /proc/crypto)"
msgstr ""
"གསང་ཨང་འདི་ནི་སྡུད་སྡེར་འདི་གསང་སྡོམ་ལ་སྤྱོད་པ།( /proc/crypto ལ་ལྟོས།)"

msgid "The hash used to create the encryption key from the passphrase"
msgstr "hashནི་གསང་ལྡེ་ལས་གསང་སྡོམ་གསང་ལྡེ་གསར་བཟོ་ལ་སྤྱོད་པ།"

msgid "The size of the device"
msgstr "སྒྲིག་ཆས་ཀྱི་ཆེ་ཆུང་།"

msgid "The size of the encryption key"
msgstr "གསང་སྡོམ་གསང་ཨང་གི་རིང་ཚད།"

msgid "The start offset in the backend device"
msgstr "རྒྱབ་སྟེགས་སྒྲིག་ཆས་སུ་སྤོ་ཚད་མགོ་རྩོམ།"

msgid ""
"This is the last keyslot. Device will become unusable after purging this "
"key."
msgstr ""
"འདི་ནི་རྗེས་མཐའི་ keyslot ཡིན།  "
"སྒྲིག་ཆས་གསང་ལྡེ་འདི་བསུབས་རྗེས་སྤྱོད་མི་རུང་།"

msgid "This will overwrite data on %s irrevocably."
msgstr "འདིས་ %s ཡིབ་ཁེབས་པའི་གཞི་གྲངས་ལེན་མི་ཐུབ།"

msgid "Timeout for interactive passphrase prompt (in seconds)"
msgstr "སྤེལ་རེས་གསང་ལྡེ་གསལ་སྟོན་གྱི་དུས་ཚོད་ཚད་བཀག(སྐར་ཆ་གྲངས།)"

msgid "Unable to obtain sector size for %s"
msgstr "%s ལ་གཡབ་ཁུལ་གྱི་ཆེ་ཆུང་ཐོབ་མི་ཐུབ།"

msgid "Unknown action."
msgstr "མི་ཤེས་པའི་བཀོལ་སྤྱོད།"

msgid "Verifies the passphrase by asking for it twice"
msgstr "ཐེངས་གཉིས་ཀྱིས་གསང་ལྡེ་ཞིབ་བཤེར་བྱ།"

msgid "[OPTION...] <action> <action-specific>]"
msgstr "[འདེམས་ཚན...] <action> <action-specific>]"

msgid "add key to LUKS device"
msgstr "གསང་ལྡེ་LUKSསྒྲིག་ཆས་ལ་སྣོན།"

msgid "create device"
msgstr "སྒྲིག་ཆས་གསར་བཟོ།"

msgid "dump LUKS partition information"
msgstr "་LUKSཁུལ་བགོད་ཆ་འཕྲིན་འདྲ་བཟོ།"

msgid "formats a LUKS device"
msgstr "རྣམ་གཞག་ཅན་དུ་LUKSསྒྲིག་ཆས་ལ་སྒྱུར།"

msgid "key %d active, purge first.\n"
msgstr "གསང་ལྡེ་ %d སྐུལ་སློང་། ཐོག་མར་བསུབ་པ།\n"

msgid "key %d is disabled.\n"
msgstr "གསང་ལྡེ་ %d བཀག་པ།\n"

msgid "key material section %d includes too few stripes. Header manipulation?\n"
msgstr ""
"གསང་ལྡེ་ཁུལ་བགོད %d ལ་མང་དྲགས་པའི་stripes ཚུད། Header "
"སྒྲིག་གཅོད་བྱ་དགོས་སམ།\n"

msgid "memory allocation error in action_luksFormat"
msgstr "ནང་གསོག་སྡེབ་སྒྲིག་action_luksFormatརུ་ནོར་འཁྲུལ་བྱུང་།"

msgid "modify active device"
msgstr "ཁུལ་བགོད་སྐུལ་སློང་སྒྲིག་འགོད།"

msgid "msecs"
msgstr "ཧའོ་སྐར་ཆ།"

msgid "open LUKS device as mapping <name>"
msgstr "LUKSསྒྲིག་ཆས་ཁ་ཕྱེ་ནས་<མིང་> ལ་རྟེན་འཕྲོ་བྱེད།"

msgid "print UUID of LUKS device"
msgstr "LUKSསྒྲིག་ཆས་ཀྱི་UUID མངོན་པ།"

msgid "remove LUKS mapping"
msgstr "LUKS རྟེན་འཕྲོ་བཤིག་འདོར་བྱེད།"

msgid "remove device"
msgstr "སྒྲིག་ཆས་འདོར་བ།"

msgid "resize active device"
msgstr "སྐུལ་སློང་གི་ཁུལ་བགོད་ཆེ་ཆུང་བཅོས་པ།"

msgid "secs"
msgstr "སྐར་ཆ།"

msgid "show device status"
msgstr "སྒྲིག་ཆས་རྣམ་པ་མངོན་པ།"

msgid "tests <device> for LUKS partition header"
msgstr "LUKSཁུལ་བགོད་མགོ་ལ་<སྒྲིག་ཆས་>ཞིབ་དཔྱད་བྱེད།"

msgid "unknown hash spec in phdr"
msgstr ""
"མི་ཤེས་པའི་ཐོར་འགྲེམས་ཁུལ་བགོད(hash spec)  ཁུལ་བགོད་མགོ(phdr) རུ་ཡོད་པ།"

msgid "unknown version %d\n"
msgstr "མི་ཤེས་པའི་པར་གཞི། %d\n"

msgid "wipes key with number <key slot> from LUKS device"
msgstr "LUKS སྒྲིག་ཆས་ནས་ཨང་ཀི<key slot>ཡིས་གསང་ལྡེ་བསུབ་པ།"
