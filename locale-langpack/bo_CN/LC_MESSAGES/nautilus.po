msgid "Stop the multi-disk drive associated with the open folder"
msgstr "ཁ་ཕྱེ་ཡིག་ཁུག་དང་འབྲེལ་བའི་སྡུད་སྡེར་མང་པོའི་སྐུལ་ཆས་མཚམས་འཇོག"

msgid "Stop the selected drive"
msgstr "བདམས་པའི་སྒྲིལ་མཚམས་འཇོག"

msgid "Stop the selected multi-disk drive"
msgstr "བདམས་པའི་སྡུད་སྡེར་མང་པོའི་སྐུལ་ཆས་མཚམས་འཇོག"

msgid "Stop the selected volume"
msgstr "བདམས་པའི་སྒྲིལ་མཚམས་འཇོག"

msgid "Stop the volume associated with the open folder"
msgstr "ཁ་ཕྱེ་ཡིག་ཁུག་དང་འབྲེལ་བའི་སྒྲིལ་མཚམས་འཇོག"

msgid "Stop the volume associated with this folder"
msgstr "ཡིག་ཁུག་འདི་དང་འབྲེལ་བའི་སྒྲིལ་མཚམས་འཇོག"

msgid "Symbolic links only supported for local files"
msgstr "མཚོན་རྟགས་སྦྲེལ་མཐུད་ཀྱིས་རང་སའི་ཡིག་ཆ་ཁོ་ན་ལ་རྒྱབ་སྐྱོར་བྱེད།"

msgid "Text"
msgstr " ཡིག་དེབ།"

msgid "Text Ellipsis Limit"
msgstr "ཡིག་རྐྱང་སྐུང་བའི་མུ་མཐའ།"

msgid "Text File"
msgstr "ཡིག་རྐྱང་ཡིག་ཆ།"

msgid "The current position of the insertion cursor in chars."
msgstr "འཇུག་ཚེག་གི་མིག་སྔའི་གནས་ས། ཡིག་རྟགས་ཀྱིས་གྲངས་བགྲང་།"

msgid "The date the file was accessed."
msgstr "ཡིག་ཆ་ལྟ་སྤྱོད་ཀྱི་དུས།"

msgid "The date the file was modified."
msgstr "ཡིག་ཆ་བཟོ་བཅོས་ཀྱི་དུས།"

msgid "The default size of an icon for a thumbnail in the icon view."
msgstr "རིས་རྟགས་མཐོང་རིས་སུ་སྐུང་རིས་ཀྱི་སོར་བཞག་རིས་རྟགས་ཆེ་ཆུང་།"

msgid "The default width of the side pane in new windows."
msgstr "སྒེའུ་ཁུང་གསར་བའི་མཐའ་ཟུར་སྡེའི་སོར་བཞག་ཞེང་ཚད།"

msgid "The destination folder is inside the source folder."
msgstr "དམིགས་ཡུལ་ཡིག་ཁུག་ཁུངས་ཡིག་ཁུག་ཏུ་གནས།"

msgid "The destination is not a folder."
msgstr "དམིགས་ཡུལ་གནས་ས་ནི་ཡིག་ཁུག་མིན།"

msgid "The destination is read-only."
msgstr "དམིགས་ཡུལ་ནི་ཀློག་ཙམ་ཡིན།"

msgid "The file is of an unknown type"
