msgid "<b>Incorrect password, please try again.</b>"
msgstr "<b>གསང་ཨང་ཡང་དག་མིན་པས་བསྐྱར་དུ་ཚོད་ལྟ་ཞིག་གནང་རོགས། </b>"

msgid "<b>Warning: </b>"
msgstr "<b>ཡི་ཐ་ཚིག：</b>\""

msgid "Command not found!"
msgstr "བཀའ་རྙེད་མ་བྱུང་།"

msgid "Command:"
msgstr "བཀའ་ལག་བསྟར།"

msgid "Do not display « ignore » button"
msgstr "སྣང་མེད་མཐེབ་མི་འཆར།"

msgid "Do not keep password"
msgstr "གསང་ཨང་ཉར་ཚགས་མི་བྱེད།"

msgid "Do not show the command to be run in the dialog"
msgstr "གླེང་སྒྲོམ་ནང་འཁོར་སྐྱོད་བྱེད་བྱའི་བཀའ་མི་འཆར།"

msgid "Fake option for KDE's KdeSu compatibility"
msgstr "KDEཡི་Kdesu དང་འཆམ་མཐུན་རྒྱུན་འཁྱུངས་ཆེད་ཀྱི་གདམ་བྱ་རྫུས་མ།"

msgid "Forget passwords"
msgstr "གསང་ཨང་བརྗེད་པ།"

msgid "Makes the dialog transient for an X app specified by winid"
msgstr ""
"Xཡིས་ཉེར་སྤྱོད་བྱ་རིམ་གླེང་སྒྲོམ་གྱི་འཕྲལ་འགྱུར་ཐང（Transient）ནི་དེའི་སྒེའུ་ཁུང་ཨང་རྟགས་ཀྱིས་དམིགས་འཛུགས་བྱེད་དུ་འཇུག"

msgid "Manual override for automatic desktop file detection"
msgstr ""
"རང་འགུལ་གྱི་ཅོག་ངོས་ཡིག་ཆར་བཤེར་འཇལ་བྱ་རྒྱུར་ལག་སྒུལ་ཁེབས་ཚད་མཁོ་འདོན་བྱེད་དགོས།"

msgid "Please enter password for <b>%1</b>."
msgstr "<b>%1</b> ཡི་གསང་ཨང་ནང་འཇུག་བྱེད་རོགས།"

msgid "Please enter your password."
msgstr "གསང་ཨང་ནང་འཇུག་བྱེད་རོགས།"

msgid "Priority:"
msgstr "སྔོན་ཐོབ་རིམ་པ།："

msgid "Process priority, between 0 and 100, 0 the lowest [50]"
msgstr ""
"\"འཕེལ་རིམ་སྔོན་ཐོབ་རིམ་པ་ནི་ 0 ནས་ 100བར་ཡིན། ཆེས་དམའ་བ་0 དང་།སོར་བཞག་གྲངས་"
" 50ཡིན།"

msgid "Specify icon to use in the password dialog"
msgstr "གསང་ཨང་གླེང་སྒྲོམ་ནང་སྤྱད་བྱའི་རིས་རྟགས་དམིགས་འཛུགས་བྱོས།"

msgid "Sudo frontend for KDE"
msgstr "KDEཡི་sudoམདུན་སྣེ།"

msgid "The command to execute"
msgstr "ལག་ཏུ་བསྟར་བྱའི་བཀའ་བརྡ།"

msgid "The comment that should be displayed in the dialog"
msgstr "གླེང་སྒྲོམ་ནང་འཆར་བྱའི་མཆན་འགྲེལ།"

msgid "Use existing DCOP server"
msgstr "ད་ཡོད་ཀྱི་DCOPཞབས་ཞུ་ཆས་བཀོལ་བ།"

msgid "Use realtime scheduling"
msgstr "དུས་ཐོག་བཀོད་གཏོང་བཀོལ་སྤྱོད།"

msgid "Use target UID if <file> is not writeable"
msgstr "གལ་སྲིད〈ཡིག་ཆ〉འབྲི་མི་ཆོག་ན་དམིགས་འབེན་UIDབཀོལ་དགོས།"

msgid "Wrong password! Exiting..."
msgstr "གསང་ཨང་ནོར་འདུག ཕྱིར་འཐེན་བྱེད་བཞིན་པ།"

msgid "Your user is not allowed to run sudo on this host!"
msgstr ""
"ཁྱོད་ཀྱི་སྤྱོད་མཁན་གྱིས་འཕྲུལ་ཆས་གཙོ་བོ་འདི་ནས་sudoའཁོར་སྐྱོད་བྱེད་དུ་མི་འཇུག"

msgid "Your user is not allowed to run the specified command!"
msgstr "ཁྱོད་ཀྱི་སྤྱོད་མཁན་གྱིས་དམིགས་འཛུགས་གྱི་བཀའ་ལག་བསྟར་བྱེད་མི་ཆོག"

msgid "Your username is unknown to sudo!"
msgstr "ཁྱེད་ཀྱི་སྤྱོད་མཁན་གྱི་མིང་sudoཁྲོད་ངེས་མེད་རེད།"

msgid "needs administrative privileges. "
msgstr "དོ་དམ་པའི་དབང་ཆ་དགོས།"

msgid "realtime:"
msgstr "དུས་ཐོག"
