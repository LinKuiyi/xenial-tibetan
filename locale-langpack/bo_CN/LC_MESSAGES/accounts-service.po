msgid "Authentication is required to change the login screen configuration"
msgstr "ཐོ་འཇུག་སྒེའུ་ཁུང་གི་བཀོད་སྒྲིག་ལ་བཟོ་བཅོས་རྒྱག་པ།དབང་ཆ་སྤྲོད་དགོས།"

msgid "Authentication is required to change user data"
msgstr "སྤྱོད་མཁན་གྱི་གཞི་གྲངས་ལ་བཟོ་བཅོས་བྱེད་པ།དབང་ཆ་སྤྲོད་དགོས།"

msgid "Authentication is required to change your own user data"
msgstr "ཁྱེད་རང་གི་སྤྱོད་མཁན་གྱི་གཞི་གྲངས་ལ་བཟོ་བཅོས་རྒྱག་པ།དབང་ཆ་སྤྲོད་དགོས།"

msgid "Change the login screen configuration"
msgstr "ཐོ་འཇུག་སྒེའུ་ཁུང་བཀོད་སྒྲིག་ལ་བཟོ་བཅོས་རྒྱག"

msgid "Change your own user data"
msgstr "ཁྱེད་རང་གི་སྤྱོད་མཁན་གྱི་གཞི་གྲངས་ལ་བཟོ་བཅོས་རྒྱག"

msgid "Enable debugging code"
msgstr "ཚབ་ཨང་ཚོད་སྒྲིག་ཁ་འབྱེད།"

msgid "Manage user accounts"
msgstr "སྤྱོད་མཁན་གྱི་རྩིས་ཐེམ་དོ་དམ།"

msgid "Output version information and exit"
msgstr "རྩིས་ཐེམ་གྱི་ཆ་འཕྲིན་ཕྱིར་གཏོང་ནས་ཕྱིར་ཐོན།"

msgid "Replace existing instance"
msgstr "མྱུར་དུ་ཡོད་པའི་ནང་དོན་བརྗེ།"
