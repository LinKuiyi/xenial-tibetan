msgid "'%s' by '%s'"
msgstr "“%2$s”ཀྱི་“%1$s”"

msgid ""
"A network error occurred, or the server closed the connection unexpectedly."
msgstr ""
"དྲ་རྒྱའི་ནོར་འཁྲུལ་བྱུང་བའམ་ཞབས་ཞུ་ཆས་ཀྱིས་ངེས་མེད་དུ་འབྲེལ་མཐུད་ཀྱི་སྒོ་རྒྱག་པ།"
" "

msgid "AUX 1 In"
msgstr "རམ་འདེགས་ནང་འཇུག1"

msgid "AUX 1 Out"
msgstr "རམ་འདེགས་ཕྱིར་འདོན།1"

msgid "AUX 2 In"
msgstr "རམ་འདེགས་ཕྱིར་འདོན།2"

msgid "AUX 2 Out"
msgstr "རམ་འདེགས་ཕྱིར་འདོན།2"

msgid "Audio playback error."
msgstr "སྒྲ་ཟློས་བསྐྱར་གཏོང་ནོར་འཁྲུལ།"

msgid "Built-in Speaker"
msgstr "ནང་བཙུགས་སྒྲ་སྒམ།"

msgid "Cannot identify device '%s'."
msgstr "སྒྲིག་ཆས་%sངེས་གཏན་བྱེད་ཐབས་བྲལ། "

msgid "Cannot operate without a clock"
msgstr "ཆུ་ཚོད་མེད་ན་བཀོལ་སྤྱོད་བྱེད་ཐབས་བྲལ།"

msgid "Cannot play stream because it is encrypted with PlayReady DRM."
msgstr ""
"བསྒྲགས་གཏོང་བྱེད་ཐབས་བྲལ། དེ་ཉིད་PlayReady DRMཡིས་གསང་སྡོམ་བྱས་ཟིན་པས་སོ།"

msgid "Changing resolution at runtime is not yet supported."
msgstr "འཁོར་སྐྱོད་རྒྱབ་སྐྱོར་མི་བྱེད་སྐབས་འབྱེད་ཕྱོད་བསྒྱུར།"

msgid "Codec Loopback"
msgstr "ཨང་སྒྲིག་ཆས་འཁོར་འཁྱིལ།"

msgid "Converter on device %s has no supported input format"
msgstr "སྒྲིག་ཆས་%sསྟེང་གི་བརྗེ་སྤོར་ཆས་ལ་ནང་འཇུག་རྣམ་པ་རྒྱབ་སྐྱོར་མེད།"

msgid "Converter on device %s has no supported output format"
msgstr "སྒྲིག་ཆས་%sསྟེང་གི་བརྗེ་སྤོར་ཆས་ལ་ཕྱིར་འདོན་རྣམ་པ་རྒྱབ་སྐྱོར་མེད།"

msgid "CoreAudio device could not be opened"
msgstr "CoreAudioསྒྲིག་ཆས་ཁ་ཕྱེ་ཐབས་བྲལ།"

msgid "CoreAudio device not found"
msgstr "CoreAudioསྒྲིག་ཆས་མ་རྙེད།"

msgid "Could not connect to server"
msgstr "ཞབས་ཞུ་ཆས་ལ་འབྲེལ་མི་ཐུབ།"

msgid "Could not dup device '%s' for reading and writing."
msgstr "སྒྲིག་ཆས་%sའདྲ་བཟོ་བྱས་ནས་ཀློག་ལེན་དང་འབྲི་ཐབས་བྲལ། "

msgid "Could not establish connection to server."
msgstr "སྒྲ་ཟློས་ཞབས་ཞུ་འཕྲུལ་ཆས་དང་འབྲེལ་མཐུད་བྱེད་ཐབས་བྲལ།"

msgid "Could not get parameters on device '%s'"
msgstr "སྒྲིག་ཆས་%sཡི་ཞུགས་གྲངས་ཐོབ་ཐབས་བྲལ།"

msgid "Could not map buffers from device '%s'"
msgstr "སྒྲིག་ཆས་%sཡི་ཁྲོད་ནས་ལྷོད་གཏོང་ཁུལ་རྟེན་འཕྲོ་བྱེད་ཐབས་བྲལ།"

msgid "Could not open audio device for playback."
msgstr "སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་གཏོང་ཐབས་བྲལ། "

msgid ""
"Could not open audio device for playback. Device is being used by another "
"application."
msgstr ""
"སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་ཕྱིར་འཇོག་ཐབས་བྲལ། "
"མིག་སྔར་བྱ་རིམ་གཞན་ཞིག་གིས་སྒྲིག་ཆས་འདི་བཀོལ་སྤྱོད་བྱེད་བཞིན་ཡོད། "

msgid ""
"Could not open audio device for playback. This version of the Open Sound "
"System is not supported by this element."
msgstr ""
"སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་བསྐྱར་གཏོང་བྱེད་ཐབས་བྲལ། པར་གཞི་འདི་ཡི་Open Sound"
" Systemཡིས་སྒྲིག་ལྷུ་འདི་ལ་རྒྱབ་སྐྱོར་མི་བྱེད། "

msgid ""
"Could not open audio device for playback. You don't have permission to open "
"the device."
msgstr ""
"སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་སྒྲ་ཟློས་གཏོང་ཐབས་བྲལ། "
"ཁྱེད་ལ་སྒྲིག་ཆས་འདི་བཀོལ་སྤྱོད་བྱེད་པའི་དབང་ཆ་མེད། "

msgid "Could not open audio device for recording."
msgstr "སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་སྒྲ་ཕབ་ཐབས་བྲལ། "

msgid ""
"Could not open audio device for recording. You don't have permission to open"
" the device."
msgstr ""
"སྒྲ་ཟློས་སྒྲིག་ཆས་ཁ་ཕྱེ་ནས་སྒྲ་ཕབ་ཐབས་བྲལ། "
"ཁྱེད་ལ་སྒྲིག་ཆས་འདི་བཀོལ་སྤྱོད་བྱེད་པའི་དབང་ཆ་མེད། "

msgid "Could not open device '%s' for reading and writing."
msgstr "སྒྲིག་ཆས་ཀྱི་%sཁ་ཕྱེ་ནས་འབྲི་ཀློག་བྱེད་ཐབས་བྲལ། "

msgid "Could not resolve server name."
msgstr "ཞབས་ཞུའི་འཕྲུལ་ཆས་ཀྱི་མིང་ལ་ཞིབ་འགྲེལ་བྱེད་ཐབས་བྲལ། "

msgid "Device '%s' cannot capture at %dx%d"
msgstr "སྒྲིག་ཆས་%sནི་%dx%dཡི་ནང་ནས་འཛིན་ཐབས་བྲལ།"

msgid "Device '%s' cannot capture in the specified format"
msgstr "སྒྲིག་ཆས་ཀྱི%sདམིགས་འཛུགས་གྱི་རྣམ་པའི་ཐོག་ནས་འཛིན་ཐབས་བྲལ།"

msgid "Device '%s' does support non-contiguous planes"
msgstr "སྒྲིག་ཆས་ཀྱི་%sཡིས་མུ་འབྲེལ་མིན་པའི་སྙོམས་ངོས་ལ་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "Device '%s' is busy"
msgstr "སྒྲིག་ཆས་%sབྲེལ་བཞིན་པའི་སྒང་རེད།"

msgid "Device '%s' is not a M2M device."
msgstr "སྒྲིག་ཆས་%sནི་M2Mསྒྲིག་ཆས་མ་རེད། "

msgid "Device '%s' is not a capture device."
msgstr "%sནི་འཛིན་བཟུང་སྒྲིག་ཆས་ཤིག་མ་རེད། "

msgid "Device '%s' is not a output device."
msgstr "%s ནི་ཕྱིར་འདོན་སྒྲིག་ཆས་མ་རེད། "

msgid "Device '%s' is not a tuner."
msgstr "སྒྲིག་ཆས་ཀྱི་%sནི་ཕྲན་སྒྲིག་མཐེབ་གནོན་མ་རེད། "

msgid "Encoder on device %s has no supported input format"
msgstr ""
"སྒྲིག་ཆས་ཀྱི་%sསྟེང་གི་ཨང་སྒྲིག་ཆས་ཀྱིས་ནང་འཇུག་རྣམ་པ་ལ་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "Encoder on device %s has no supported output format"
msgstr ""
"སྒྲིག་ཆས་ཀྱི་%sསྟེང་གི་ཨང་སྒྲིག་ཆས་ཀྱིས་ཕྱིར་འདོན་རྣམ་པ་ལ་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "Error getting capabilities for device '%s'."
msgstr "སྒྲིག་ཆས་ཀྱི%sའབྲེལ་ཡོད་ཆ་འཕྲིན་ཐོབ་སྐབས་ནོར་འཁྲུལ་ཤོར།"

msgid ""
"Error getting capabilities for device '%s': It isn't a v4l2 driver. Check if"
" it is a v4l1 driver."
msgstr ""
"སྒྲིག་ཆས་ཀྱི%sཡི་ཆ་འཕྲིན་ཐོབ་སྐབས་ནོར་འཁྲུལ་ཤོར།དེ་ནི་v4l2སྐུལ་ཆས་ཤིག་མིན་པས།v4l1ཡིན་མིན་ལ་ཞིབ་བཤེར་བྱེད་རོགས།"

msgid "Error reading %d bytes from device '%s'."
msgstr "སྒྲིག་ཆས་ཀྱི%2$sཡི་ཁྲོད་ནས་%1$dཡིག་འབྲུ་ཀློག་སྐབས་ནོར་འཁྲུལ་ཤོར་བ།"

msgid "Error recording from audio device."
msgstr "སྒྲ་ཟློས་སྒྲིག་ཆས་སྒྲ་ཕབ་ནོར་འཁྲུལ།"

msgid "Failed getting controls attributes on device '%s'."
msgstr "སྒྲིག་ཆས་ཀྱི་%s སྟེང་གི་ཚོད་འཛིན་གཏོགས་གཤིས་ཐོབ་སྐབས་ནོར་འཁྲུལ་བྱུང་།"

msgid "Failed to allocate required memory."
msgstr "བགོ་འགྲེམས་རེ་ཞུ་བྱེད་པའི་ནང་གསོག་ཕམ་པ།"

msgid "Failed to allocated required memory."
msgstr "བགོ་འགྲེམས་རེ་ཞུ་བྱེད་པའི་ནང་གསོག་ཕམ་པ།"

msgid "Failed to change mute state for device '%s'."
msgstr "སྒྲིག་ཆས་ཀྱི%sཡི་སྒྲ་མེད་རྣམ་པ་བསྒྱུར་སྐབས་ནོར་འཁྲུལ་བྱུང་བ།"

msgid "Failed to configure internal buffer pool."
msgstr "ནང་ཁུལ་གྱི་ཞི་ལྷོད་རྫིང་བུ་སྡེབ་སྒྲིག་བྱེད་ཐབས་བྲལ།"

msgid "Failed to decode JPEG image"
msgstr "JPEGབརྙན་རིས་ཨང་འགྲོལ་ནོར་འཁྲུལ་བྱུང་བ།"

msgid "Failed to enumerate possible video formats device '%s' can work with"
msgstr ""
"སྒྲིག་ཆས་ཀྱི%sཕལ་ཆེར་རྒྱབ་སྐྱོར་བྱེད་སྲིད་པའི་བརྙན་ཟློས་རྣམ་གཞག་དཔེ་བགྲངས་སྐབས་ནོར་འཁྲུལ་བྱུང་བ།"

msgid ""
"Failed to get current input on device '%s'. May be it is a radio device"
msgstr ""
"སྒྲིག་ཆས་%sཐོག་གི་མིག་སྔའི་ནང་འཇུག་ཐོབ་ལེན་སྐབས་ནོར་འཁྲུལ་བྱུང་བ། "
"གཅིག་བྱས་ན་དེ་ནི་རླུང་འཕྲིན་སྒྲིག་ཆས་ཤིག་རེད།"

msgid ""
"Failed to get current output on device '%s'. May be it is a radio device"
msgstr ""
"སྒྲིག་ཆས་%sཐོག་གི་མིག་སྔའི་ནང་འཇུག་ཐོབ་ལེན་སྐབས་ནོར་འཁྲུལ་བྱུང་བ། "
"དེ་ནི་རྒྱང་བསྒྲགས་སྒྲིག་ཆས་ཤིག་ཡིན་ཤས་ཆེ།"

msgid "Failed to get current tuner frequency for device '%s'."
msgstr ""
"སྒྲིག་ཆས་ཀྱི་%s "
"ལེན་པའི་སྐབས་ཀྱི་མིག་སྔའི་ལེགས་སྒྲིག་མཐེབ་གནོན་གྱི་ཟློས་ཕྱོད་ལ་ནོར་འཁྲུལ་བྱུང་བ།"

msgid "Failed to get setting of tuner %d on device '%s'."
msgstr ""
"སྒྲིག་ཆས་%2$s སྟེང་གི་ལེགས་སྒྲིག་མཐེབ་གནོན་%1$d "
