msgid "Ignore for now"
msgstr "ད་ལྟ་སྣང་མེད་དུ་བཞག"


msgid "Hide"
msgstr "སྦེད་པ།"

msgid "Install Packages"
msgstr "མཉེན་ཆས་ཐུམ་སྒྲིག་འཇུག"

msgid ""
"Installation complete. You will need to restart %1 to use the new "
"functionality"
msgstr ""
"སྒྲིག་སྦྱོར་བྱས་ཟིན།  ཁྱེད་ཀྱིས  %1 "
"བསྐྱར་སློང་བྱས་ན་ད་གཟོད་བྱེད་ནུས་གསར་པར་ནུས་པ་ཐོན་ཐུབ།"

msgid "Install"
msgstr "སྒྲིག་འཇུག"

msgid "K3b CD Codecs"
msgstr "K3b CD ཨང་དགྲོལ།"

msgid "Manage Drivers"
msgstr "སྐུལ་སློང་བྱ་རིམ་དོ་དམ་བྱེད་པ།"

msgid "MP3 Encoding"
msgstr "MP3 ཨང་དགྲོལ།"

msgid "Extra packages can be installed to enhance functionality for %1"
msgstr ""
"%1 གི་བྱེད་ནུས་ཆེ་རུ་གཏོང་ཆེད་ཚད་བརྒལ་གྱི་མཉེན་ཆས་ཐུམ་སྒྲིག་འཇུག་བྱས་ཆོག"

msgid "Proprietary drivers might be required to enable additional features"
msgstr ""
"ཁྱད་ཆོས་ཆེར་གཏོང་སྤྱོད་འགོ་རྩོམ་ན་ཆེད་ཡོད་སྐུལ་སློང་གི་རྒྱབ་སྐྱོར་ཡོད་དགོས།"

msgid "Language support is incomplete, additional packages are required"
msgstr "སྐད་བརྡའི་རྒྱབ་སྐྱོར་འཐུས་ཚང་མིན། ཚད་བརྒལ་གྱི་མཉེན་ཆས་ཐུམ་མ་དགོས།"

msgid "Software upgrade notifications are available"
msgstr ""

msgid "An application has crashed on your system (now or in the past)"
msgstr ""
"ཁྱེད་ཀྱི་རྒྱུད་ཁོངས་ཁྲོད་ནས་བཀོལ་སྤྱོད་བྱ་རིམ་ཞིག་རྩ་ཐོར་དུ་སོང་ཡོད།(ད་ལྟའམ་ཡང་ན་དེ་སྔོན།)"

msgid "A system restart is needed to complete the update process"
msgstr "རྒྱུད་ཁོངས་བསྐྱར་སློང་བྱས་ནས་བཀོད་སྤྱོད་ལེགས་འགྲུབ་བྱེད་པ།"

msgid "Details"
msgstr "ཞིབ་ཕྲའི་ཆ་འཕྲིན།"

msgid "Restart"
msgstr "བསྐྱར་སློང་།"

msgid "Run this action now"
msgstr "ད་ལྟ་བྱ་འགུལ་འདི་ལག་བསྟར་བྱེད་པ།"

msgid ""
"Select packages to be installed for extra functionality. These packages are "
"not installed by default due to either patent issues, restrictive licensing "
"or a lack of space on the installation media."
msgstr ""

msgid "System Notification Helper"
msgstr "རྒྱུད་ཁོངས་བརྡ་ཐོ་ལས་རོགས།"

msgid "Flash"
msgstr "འོད་འཁྱུག་གློག་སྒྲོན།"

msgid "Update Information"
msgstr "རིམ་སྤོར་ཆ་འཕྲིན།"

msgid "Ignore"
msgstr "སྣང་མེད།"

msgid "Never show again"
msgstr "མི་མངོན་པ།"

