msgstr "བསམ་འཆར།"

msgid "syntax error in method options file '%.250s' -- %s"
msgstr ""
"སྒྲིག་འཇུག་ལག་ཐབས་ཀྱི་འདེམས་ཚན་ཡིག་ཆ་“%.250s” ནང་དུ་བརྡ་སྤྲོད་ནོར་འཁྲུལ་ -- "
"%s ཡོད།"

msgid "terminal lacks necessary features, giving up"
msgstr "མཐའ་སྣེ་ལ་དགོས་ངེས་ཀྱི་ཁྱད་ཆོས་ཆད་པས། བཀོལ་སྤྱོད་མཚམས་འཇོག་པ།"

msgid "unable to access method script '%.250s'"
msgstr "ལག་ཐབས་ཞབས་དེབ་“%.250s”ལ་ལྟ་སྤྱོད་མི་ཐུབ།"

msgid "unable to open current option file '%.250s'"
msgstr "མིག་སྔའི་འདེམས་ཚན་ཡིག་ཆ་“%.250s”ཁ་འབྱེད་མི་ཐུབ།"

msgid "unable to open option description file '%.250s'"
msgstr "འདེམས་ཚན་གསལ་བཤད་ཡིག་ཆ་“%.250s”ཁ་འབྱེད་མི་ཐུབ།"

msgid "unable to read '%.250s' directory for reading methods"
msgstr ""
"“%.250s”དཀར་ཆག་བཀླགས་ནས་སྒྲིག་འཇུག་ལག་ཐབས་ཀྱི་འབྲེལ་ཡོད་ཆ་འཕྲིན་ཐོབ་མི་ཐུབ།"

msgid "unable to read method options file '%.250s'"
msgstr "ལག་ཐབས་ཞབས་དེབ་“%.250s”ཀློག་མི་ཐུབ།"

msgid "unable to stat option description file '%.250s'"
msgstr "འདེམས་ཚན་གསལ་བཤད་ཡིག་ཆ་“%.250s” stat མི་ཐུབ།"

msgid "unable to write new option to '%.250s'"
msgstr "འདེམས་ཚན་ཡིག་ཆ་“%.250s”གསར་པ་འབྲི་མི་ཐུབ།"

msgid "unknown action string '%.50s'"
msgstr "ངོས་འཛིན་མི་ཐུབ་པའི་བཀོལ་སྤྱོད་ཡིག་ཕྲེང་“%.50s”"

msgid "unpacked (not set up)"
msgstr "བཙིར་གནོན་འགྲོལ་བ།(སྒྲིག་འགོད་བྱས་མེད།)"

msgid "update available list script"
msgstr "སྤྱོད་རུང་མཉེན་ཆས་ཐུམ་རེའུ་མིག་གི་ཞབས་དེབ་གསར་སྒྱུར།"
