msgid "A system log viewer for GNOME."
msgstr "GNOME རྒྱུད་ཁོངས་ཉིན་ཐོའི་ལྟ་ཆས།"

msgid "About System Log"
msgstr "རྒྱུད་ཁོངས་ཉིན་ཐོའི་སྐོར།"

msgid "Add new filter"
msgstr "འཚག་ཆས་གསར་བསྣན།"

msgid "Auto Scroll"
msgstr "རང་འགུལ་འགྲིལ་བ།"

msgid "Background:"
msgstr "རྒྱབ་ལྗོངས།："

msgid "Can't read from \"%s\""
msgstr "%sནས་ཀློག་མི་ཆོག"

msgid "Close"
msgstr "ཁ་རྒྱག་པ།"

msgid "Copy"
msgstr "འདྲ་ཕབ།"

msgid "Could not open the following files:"
msgstr "གཤམ་གསལ་གྱི་ཡིག་ཆ་ཁ་ཕྱེ་ཐབས་བྲལ་བ།"

msgid "Could not parse arguments"
msgstr "ཞུགས་གྲངས་དབྱེ་ཞིབ་བྱེད་ཐབས་མེད།"

msgid "Could not register the application"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་ཐོ་འགོད་བྱེད་ཐབས་བྲལ།"

msgid "Edit filter"
msgstr "འཚག་ཆས་རྩོམ་སྒྲིག"

msgid "Effect:"
msgstr "ཕན་འབྲས།"

msgid "Error while uncompressing the GZipped log. The file might be corrupt."
msgstr ""
"GZippedཡི་ཉིན་ཐོ་བཙིར་གནོན་སྐབས་ནོར་འཁྲུལ་བྱུང་། "
"ཡིག་ཆ་དེ་ལ་སྐྱོན་བྱུང་ཡོད་སྲིད།"

msgid "Filter name is empty!"
msgstr "འཚག་ཆས་ཀྱི་མིང་སྟོང་རེད།"

msgid "Filter name may not contain the ':' character"
msgstr "འཚག་ཆས་ཀྱི་མིང་ལ་ཡིག་རྟགས“:”འདུས་མི་རུང་།"

msgid "Filters"
msgstr "ཡིག་ཆ།"

msgid "Find next occurrence of the search string"
msgstr "འཚོལ་བཤེར་ཡིག་རྟགས་ཕྲེང་ཕྱིའི་ཐོན་ཐེངས་འཚོལ།"

msgid "Find previous occurrence of the search string"
msgstr "འཚོལ་བཤེར་ཡིག་རྟགས་ཕྲེང་སྔའི་ཐོན་ཐེངས་འཚོལ།"

msgid "Foreground:"
msgstr "མདུན་ལྗོངས།："

msgid "Height of the main window in pixels"
msgstr "སྒེའུ་ཁུང་གཙོ་བོའི་མཐོ་ཚད།བརྙན་རྒྱུས་རྩི་བ།"

msgid "Help"
msgstr " རོགས་རམ།"

msgid "Hide"
msgstr "གབ་པ།"

msgid "Highlight"
msgstr "མདངས་ཆེན།"

msgid "Impossible to open the file %s"
msgstr "ཡིག་ཆ %s ཁ་ཕྱེ་ཐབས་བྲལ།"

msgid "List of saved filters"
msgstr "ཉར་ཚགས་བྱས་ཟིན་པའི་འཚག་ཆས་རེའུ་མིག"

msgid "List of saved regexp filters"
msgstr "ཚད་མཐུན་མཚོན་ཚུལ་གྱི་འཚག་ཆས་ཉར་ཚགས་བྱེད་པའི་རེའུ་མིག"

msgid "Loading..."
msgstr "འགེལ་བཞིན་པ།"

msgid "Log file to open up on startup"
msgstr "འགོ་སློང་སྐབས་སྒོ་འབྱེད་པའི་ཉིན་ཐོའི་ཡིག་ཆ།"

msgid "Log files to open up on startup"
msgstr "འགོ་སློང་སྐབས་སྒོ་འབྱེད་པའི་ཉིན་ཐོའི་ཡིག་ཆ།"

msgid "Manage Filters..."
msgstr "དོ་དམ་འཚག་ཆས།"

msgid "No matches found"
msgstr "ཟླ་སྒྲིག་གི་ཚན་པ་རྙེད་མེད་པ།"

msgid "Normal Size"
msgstr "གདོད་མའི་ཆེ་ཆུང་།"

msgid "Open Log"
msgstr "ཉིན་ཐོའི་ཁ་འབྱེད་པ།"

msgid "Open..."
msgstr "ཁ་ཕྱེ་བ།"

msgid "Please specify either foreground or background color!"
msgstr "མདུན་ལྗོངས་སམ་ཡང་ན་རྒྱབ་ལྗོངས་ཀྱི་ཁ་དོག་དམིགས་འཛུགས་གནང་དགོས།"

msgid "Quit"
msgstr " ཕྱིར་འབུད།"

msgid "Regular expression is empty!"
msgstr "ཚད་མཐུན་གྱི་མཚོན་ཚུལ་སྟོང་པ་རེད།"

msgid "Regular expression is invalid: %s"
msgstr "ནུས་མེད་ཀྱི་ཚད་མཐུན་མཚོན་ཚུལ།：%s"

msgid "Search in \"%s\""
msgstr "“%s” ཡི་ནང་བཤེར་འཚོལ་བྱེད་པ།"

msgid "Select All"
msgstr "ཡོད་ཚད་འདེམ་པ།"

msgid "Show Matches Only"
msgstr "ཟླ་སྒྲིག་ཚན་བྱང་ཁོ་ན་འཆར་བ།"

msgid "Show the version of the program."
msgstr "བྱ་རིམ་གྱི་པར་གཞི་མངོན་པ།"

msgid "Size of the font used to display the log"
msgstr "ཉིན་ཐོའི་ཡིག་གཟུགས་ཆེ་ཆུང་འཆར་བྱེད་དུ་སྤྱོད་པ།"

msgid "Specifies the height of the log viewer main window in pixels."
msgstr "ཉིན་ཐོ་ལྟ་ཆས་སྒེའུ་ཁུང་གི་མཐོ་ཚད་དམིགས་འཛུགས།བརྙན་རྒྱུས་བརྩི་བ།"

msgid "Specifies the width of the log viewer main window in pixels."
msgstr "ཉིན་ཐོ་ལྟ་ཆས་སྒེའུ་ཁུང་གི་ཞེང་ཚད་དམིགས་འཚུགས།བརྙན་རྒྱུས་བརྩི་བ།"

msgid "System Log"
msgstr "རྒྱུད་ཁོངས་ཉིན་ཐོ།"

msgid "The file is not a regular file or is not a text file."
msgstr ""
"ཡིག་ཆ་ནི་རྒྱུན་ལྡན་གྱི་ཡིག་ཆ་ཞིག་མིན་པའམ་ཡང་ན་ཡིག་དེབ་ཡིག་ཆ་ཞིག་མ་རེད།"

msgid "There was an error displaying help: %s"
msgstr "རོགས་རམ་བྱེད་སྐབས་བྱུང་བའི་ནོར་འཁྲུལ་འཆར་བ།：%s"

msgid "This version of System Log does not support GZipped logs."
msgstr ""
"རྒྱུད་ཁོངས་འདིའི་ཉིན་ཐོའི་པར་གཞི་ཡིས་GZippedཉིན་ཐོ་ལ་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "View or monitor system log files"
msgstr "རྒྱུད་ཁོངས་གི་ཉིན་ཐོའི་ཡིག་ཆ་ལྟ་བའམ་སོ་ལྟ་བྱེད་པ།"

msgid "Width of the main window in pixels"
msgstr "སྒེའུ་ཁུང་གཙོ་བོའི་ཞེང་ཚད། བརྙན་རྒྱུས་རྩིས་པ།"

msgid "Wrapped"
msgstr "ཕྲེང་བརྗེ་བ།"

msgid "You don't have enough permissions to read the file."
msgstr "ཁྱོད་ལ་ཡིག་ཆ་འདི་བཀླགས་པའི་དབང་ཆ་མེད་པ།"

msgid "Zoom In"
msgstr "ཇེ་ཆེར་གཏོང་བ།"

msgid "Zoom Out"
msgstr "ཆུང་བསྡུ།"

msgid "[URI...]"
msgstr "[URI...]"

msgid "_Add"
msgstr "སྣོན་པ། (A)"

msgid "_Cancel"
msgstr " དོར་བ།"

msgid "_Close"
msgstr "ཁ་རྒྱག (C)"

msgid "_Name:"
msgstr "མིང་།(_N)："

msgid "_Open"
msgstr "ཁ་འབྱེད།(O)"

msgid "_Properties"
msgstr "གཏོགས་གཤིས། (_P)"

msgid "_Regular Expression:"
msgstr "ཚད་མཐུན་མཚོན་ཚུལ།(_R)："

msgid "_Remove"
msgstr "སྤོ་བ།（R)"

msgid "logs;debug;error;"
msgstr "logs;debug;error; རྒྱུད་ཁོངས་ཉིན་ཐོ། ནོར་འཁྲུལ། ཚོད་སྒྲིག"

msgid "today"
msgstr "དེ་རིང་།"

msgid "updated"
msgstr "གསར་སྒྱུར་བྱས་ཟིན།"

msgid "yesterday"
msgstr "ཁ་སང་།"
