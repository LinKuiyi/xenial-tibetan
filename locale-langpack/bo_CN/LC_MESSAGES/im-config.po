msgid "Bogus Configuration"
msgstr "མེད་པའི་སྡེབ་སྒྲིག"

msgid "Custom Configuration"
msgstr "མཚན་ཉིད་རང་འཇོག་སྡེབ་སྒྲིག"

msgid ""
"Custom configuration is created by the user or administrator using editor."
msgstr ""
"བེད་སྤྱོད་མཁན་ཡང་ན་དོ་དམ་པས་སྒྲིག་ཆས་བརྒྱུད་ནས་བྱུང་བའི་མཚན་ཉིད་རང་འཇོག་སྒྲིག་འགོད།"

msgid ""
"E: $IM_CONFIG_NAME is bogus configuration for $IM_CONFIG_XINPUTRC. Doing "
"nothing."
msgstr ""
"E: $IM_CONFIG_NAME ནི་ཟོལ་མའི་ $IM_CONFIG_XINPUTRC སྡེབ་སྒྲིག་རེད། "
"ཅི་ཡང་མི་སྒྲུབ་པ།"

msgid "E: Configuration for $IM_CONFIG_NAME not found at $IM_CONFIG_CODE."
msgstr ""
"E: $IM_CONFIG_NAME ཡིས་སྤྱད་པའི་སྒྲིག་འགོད་$IM_CONFIG_CODE ནས་མི་རྙེད་པ།"

msgid ""
"E: Configuration in $IM_CONFIG_XINPUTRC is manually managed. Doing nothing."
msgstr ""
"E: $IM_CONFIG_XINPUTRC ཁྲོད་ཀྱི་སྡེབ་སྒྲིག་ནི་ལག་སྒུལ་དོ་དམ་རེད། "
"ཅི་ཡང་མི་སྒྲུབ་པ།"

msgid "E: Script for $IM_CONFIG_NAME not found at $IM_CONFIG_CODE."
msgstr ""
"E: $IM_CONFIG_NAME ཡིས་སྤྱད་པའི་script དེ་ $IM_CONFIG_CODE ནས་མི་རྙེད་པ།"

msgid "E: X server must be available."
msgstr "E: X ཞབས་ཞུ་ཆས་ངེས་པར་དུ་སྤྱོད་རུང་བ།"

msgid "E: zenity must be installed."
msgstr "E: ངེས་པར་དུ་zenityསྒྲིག་འཇུག་བྱེད་དགོས།"

msgid "Input Method Configuration (im-config, ver. $IM_CONFIG_VERSION)"
msgstr "འཇུག་ཐབས་སྡེབ་སྒྲིག(im-config པར་གཞི། $IM_CONFIG_VERSION)"

msgid "Keeping the $IM_CONFIG_XINPUTRC_TYPE $IM_CONFIG_XINPUTRC as missing."
msgstr ""
"$IM_CONFIG_XINPUTRC_TYPEདང་ $IM_CONFIG_XINPUTRC དེ་ missingརུ་སོར་འཇོག"

msgid "Missing"
msgstr "མི་འདང་བ།"

msgid "Missing configuration file."
msgstr "སྡེབ་སྒྲིག་གི་ཡིག་ཆ་ཆད་པ།"

msgid "Non existing configuration name is specified."
msgstr "མེད་པའི་སྡེབ་སྒྲིག་གི་མིང་དམིགས་འཛུགས་བྱས་པ།"

msgid "Removing the $IM_CONFIG_XINPUTRC_TYPE $IM_CONFIG_XINPUTRC."
msgstr " $IM_CONFIG_XINPUTRC_TYPE དང་ $IM_CONFIG_XINPUTRCསྤོ་འབུད།"

msgid ""
"Select $IM_CONFIG_XINPUTRC_TYPE. The user configuration supersedes the "
"system one."
msgstr ""
"$IM_CONFIG_XINPUTRC_TYPEའདེམ་པ། "
"སྤྱོད་མཁན་སྒྲིག་འགོད་ཀྱིས་རྒྱུད་ཁོངས་སྒྲིག་འགོད་འགེབ་པ།"

msgid ""
"This activates the bare XIM with the X Keyboard Extension for all softwares."
msgstr ""
"མཉེན་ཆས་ཡོད་ཚད་ལ་Xམཐེབ་གཞོང་གིས་རྒྱ་བསྐྱེད་པའི་XIMརྐྱང་པ་ཡོད་ཚད་སྐུལ་སློང་བྱེད་པ།"

msgid "activate Chinese input method (gcin)"
msgstr "gcinརྒྱ་ཡིག་འཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate Flexible Input Method Framework (fcitx)"
msgstr "བྱ་རྐང་ཐང་འཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate HIME Input Method Editor (hime)"
msgstr "HIMEའཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate Hangul (Korean) input method"
msgstr "Hangul(ཁོར་ཡིག)འཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate Intelligent Input Bus (IBus)"
msgstr "IBus རིག་ནུས་འཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate Mallit input method framework"
msgstr "Mallitའཇུག་ཐབས་སྒྲོམ་གཞི་སྐུལ་སློང་།"

msgid "activate Smart Common Input Method (SCIM)"
msgstr "Smart Common Input Method(SCi)འགོ་སློང་།"

msgid "activate Thai input method with thai-libthai"
msgstr "thai-libthaiཐེ་ཡིག་འཇུག་ཐབས་སྤྱོད་འགོ་རྩོམ་པ།"

msgid "activate XIM for Chinese with Sunpinyin"
msgstr "SunpinyXIMརྒྱ་ཡིག་འཇུག་ཐབས་སྤྱོད་པ།"

msgid "activate XIM for Japanese with kinput2"
msgstr "kinput2XIMཉི་ཧོང་སྐད་ཀྱི་འཇུག་ཐབས་སྤྱོད་པ།"

msgid "activate the bare XIM with the X Keyboard Extension"
msgstr "Xམཐེབ་གཞོང་གིས་རྒྱ་སྐྱེད་པའི་XIMརྐྱང་པ་སྐུལ་སློང་།"

msgid "activate universal input method (uim)"
msgstr " universal input method། (uim)འགོ་སློང་།"

msgid "description"
msgstr " ཞིབ་བརྗོད།"

msgid "do not set any IM from im-config"
msgstr "im-configབརྒྱུད་ནས་འཇུག་ཐབས་གང་ཡང་སྒྲིག་འགོད་མི་བྱེད།"

msgid "name"
msgstr " མིང་།"

msgid "remove IM $IM_CONFIG_XINPUTRC_TYPE $IM_CONFIG_XINPUTRC"
msgstr "$IM_CONFIG_XINPUTRC_TYPE $IM_CONFIG_XINPUTRCསྤོ་འབུད།"

msgid "select"
msgstr "འདེམ་པ།"

msgid "system configuration"
msgstr "རྒྱུད་ཁོངས་སྒྲིག་འགོད།"

msgid "use $IM_CONFIG_DEFAULT_MODE mode (bogus content in $IM_CONFIG_DEFAULT)"
msgstr ""
"$IM_CONFIG_DEFAULT_MODE རྣམ་པ་སྤྱད་ན་($IM_CONFIG_DEFAULT "
"ཁྲོད་ཀྱི་ནང་དོན་ལ་གནད་དོན་ཡོད་པ།)"

msgid "use $IM_CONFIG_DEFAULT_MODE mode (missing $IM_CONFIG_DEFAULT )"
msgstr "使用 $IM_CONFIG_DEFAULT_MODE རྣམ་པ་སྤྱོད་པ།($IM_CONFIG_DEFAULTཆད།)"

msgid "use $IM_CONFIG_DEFAULT_MODE mode set by $IM_CONFIG_DEFAULT"
msgstr ""
"$IM_CONFIG_DEFAULT སྒྲིག་འགོད་ཀྱི་ $IM_CONFIG_DEFAULT_MODE རྣམ་པ་སྤྱོད་པ།"

msgid "use auto mode only under CJKV"
msgstr "CJKVཁོར་ཡུག་ཁོ་ནའི་འོག་རང་འགུལ་རྣམ་པ་སྤྱོད་ཆོག"

msgid "user configuration"
msgstr "སྤྱོད་མཁན་སྒྲིག་འགོད།"
