msgid "%s's public files"
msgstr "%s ཡི་ཁྲོམ་བསྒྲགས་ཡིག་ཆ།"

msgid "%s's public files on %s"
msgstr "%s དེ %s ཡི་སྟེང་གི་ཁྲོམ་བསྒྲགས་ཡིག་ཆ།"

msgid "Accept"
msgstr "སྡུད་ལེན།"

msgid "Always"
msgstr "ནམ་ཡང་།"

msgid "Ask"
msgstr "འདྲི་རྩད།"

msgid "Cancel"
msgstr " དོར་བ།"

msgid "Could not build interface."
msgstr "མཚམས་ངོས་བཟོ་ཐབས་བྲལ།"

msgid "Could not display the help contents."
msgstr "རོགས་རམ་དཀར་ཆག་མངོན་ཐབས་བྲལ།"

msgid "File Received"
msgstr "ཡིག་ཆ་སྡུད་ལེན་ཟིན་པ།"

msgid "File reception complete"
msgstr "ཡིག་ཆ་སྡུད་ལེན་ལེགས་གྲུབ་བྱུང་བ།"

msgid ""
"If this is true, the Public directory in the users home directory will be "
"shared over the network when the user is logged in."
msgstr ""
"གལ་སྲིད་ངོ་མ། (true) ཡིན་ཚེ། "
"སྤྱོད་མཁན་ཐོ་འགོད་སྐབས་སྤྱོད་མཁན་གྱི་དཀར་ཆག་གཙོ་བོའི་ནང་གི་དཀར་ཆག "
"“ཁྲོམ་བསྒྲགས ” དྲ་རྒྱའི་ནང་མཉམ་སྤྱོད་བྱེད་པ་ཡིན།"

msgid "Launch Bluetooth ObexPush sharing if enabled"
msgstr ""
"གལ་སྲིག་སྤྱད་ཚེ། སོ་སྔོན་ཡིག་ཆའི་བརྒྱུད་གཏོང་མཉམ་སྤྱོད་འགོ་སློང་བ་ཡིན།"

msgid "Launch Personal File Sharing if enabled"
msgstr "གལ་སྲིད་སྤྱད་ཚེ། མི་སྒེར་གྱི་ཡིག་ཆ་མཉམ་སྤྱོད་འགོ་སློང་བ་ཡིན།"

msgid "May be shared over the network"
msgstr "དྲ་རྒྱ་བརྒྱུད་མཉམ་སྤྱོད་བྱས་ཆོག"

msgid "Never"
msgstr "གཏན་མིན།"

msgid "No reason"
msgstr "རྒྱུ་མཚན་མེད་པ།"

msgid "OK"
msgstr " གཏན་འཁེལ།"

msgid "Only for set up devices"
msgstr "བསྡམས་པའི་སྒྲིག་ཆས་ཁོ་ནར།"

msgid "Open"
msgstr "ཁ་ཕྱེ་བ།"

msgid "Open File"
msgstr " ཡིག་ཆའི་ཁ་འབྱེད་པ།"

msgid "Personal File Sharing"
msgstr "མི་སྒེར་གྱི་ཡིག་ཆ་མཉམ་སྤྱོད།"

msgid "Personal File Sharing Preferences"
msgstr "མི་སྒེར་གྱི་ཡིག་ཆ་མཉམ་སྤྱོད་ཀྱི་འདེམས་ཚན།"

msgid "Preferences"
msgstr "མཐོ་རིམ།"

msgid "Receive"
msgstr "སྡུད་ལེན།"

msgid "Receive Files over Bluetooth"
msgstr "སོ་སྔོན་བརྒྱུད་སྐྱེལ་བའི་ཡིག་ཆ་སྡུད་ལེན།"

msgid "Receive files in _Downloads folder over Bluetooth"
msgstr ""
"སོ་སྔོན་བརྒྱུད་སྐྱེལ་བའི་ཡིག་ཆ་དེ་ཡིག་ཁུག “ཕབ་ལེན” ཡི་ནང་དུ་སྡུད་ལེན་བྱེད་པ།"
" (_D)"

msgid "Reject"
msgstr "ཟློག་པ།"

msgid "Reveal"
msgstr "མངོན་པ།"

msgid "Reveal File"
msgstr "ཡིག་ཆ་མངོན་པ།"

msgid "Share Files over the Network"
msgstr "དྲ་རྒྱའི་ནང་ཡིག་ཆ་མཉམ་སྤྱོད།"

msgid "Share Public directory over the network"
msgstr "དྲ་རྒྱའི་ནང་དཀར་ཆག “ཁྲོམ་བསྒྲགས་” མཉམ་སྤྱོད།"

msgid "Sharing"
msgstr " མཉམ་སྤྱོད།"

msgid "Sharing Settings"
msgstr "མཉམ་སྤྱོད་སྒྲིག་འགོད།"

msgid ""
"This feature cannot be enabled because the required packages are not "
"installed on your system"
msgstr ""
"དགོས་ངེས་ཀྱི་མཉེན་ཆས་ཐུམ་བུ་སྒྲིག་འཇུག་བྱས་མེད་པས་ཁྱད་གཤིས་འདི་སྤྱོད་ཐབས་བྲལ།"

msgid "When to accept files sent over Bluetooth"
msgstr "སོ་སྔོན་བརྒྱུད་སྐྱེལ་བའི་ཡིག་ཆ་དེ་ག་དུས་སྡུད་ལེན་བྱེད་དམ།"

msgid ""
"When to ask for passwords. Possible values are \"never\", \"on_write\", and "
"\"always\"."
msgstr ""
"གསང་ཨང་ག་དུས་འདྲི་རྩད་བྱེད་དམ། འདེམས་རུང་གི་འདེམས་ཚན་ནི：“never” དང་། "
"“on_write” “always” བཅས་ཡིན།"

msgid "When to require passwords"
msgstr "གསང་ཨང་ག་དུས་ཐོབ་བམ།"

msgid "When writing files"
msgstr "ཡིག་ཆ་འབྲི་སྐབས།"

msgid "Whether Bluetooth clients can send files using ObexPush."
msgstr "སོ་སྔོན་དུད་སྣེས ObexPus བེད་སྤྱད་དེ་ཡིག་ཆ་སྐྱེལ་ཆོག་གམ།"

msgid "Whether to notify about newly received files."
msgstr "གསར་དུ་འབྱོར་བའི་ཡིག་ཆའི་སྐོར་གྱི་བརྡ་ཐོ་གཏོང་དགོས་སམ།"

msgid "You have been sent a file"
msgstr "ཁྱེད་ཀྱིས་ཡིག་ཆ་ཞིག་སྐྱེལ་བ།"

msgid "You have been sent a file \"%s\" via Bluetooth"
msgstr "ཁྱེད་ཀྱིས་སོ་སྔོན་བརྒྱུད་ཡིག་ཆ “%s” སྐྱེལ་བ།"

msgid "You have received the file “%s” over Bluetooth."
msgstr "ཁྱེད་ཀྱིས་སོ་སྔོན་བརྒྱུད་ཡིག་ཆ “%s” སྡུད་ལེན་བྱས་པ།"

msgid "You received \"%s\" via Bluetooth"
msgstr "སོ་སྔོན་བརྒྱུད “%s” འབྱོར་བ།"

msgid "You received a file"
msgstr "ཡིག་ཆ་ཞིག་འབྱོར་བ།"

msgid "_Accept files: "
msgstr "ཡིག་ཆ་སྡུད་ལེན། (_A)："

msgid "_Notify about received files"
msgstr "འབྱོར་ཟིན་པའི་ཡིག་ཆའི་སྐོར་གྱི་བརྡ་ཐོ།(_N)"

msgid "_Password:"
msgstr "གསང་གྲངས།(_P)："

msgid "_Require password:"
msgstr "གསང་ཨང་དགོས་པ། (_R)"

msgid "_Share public files on network"
msgstr "དྲ་རྒྱའི་ནང་ཁྲོམ་བསྒྲགས་ཡིག་ཆ་མཉམ་སྤྱོད། (_S)"
