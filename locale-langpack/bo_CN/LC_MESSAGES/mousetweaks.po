msgid "- GNOME mouse accessibility daemon"
msgstr "--GNOMEཙིག་རྟགས་ཀྱི་གཞོགས་འདེགས་བྱེད་ནུས་སྲུང་སྐྱོང་བྱ་རིམ།"

msgid "Button Style"
msgstr "མཐེབ་གནོན་རྣམ་པ།"

msgid "Button style of the click-type window."
msgstr "རིགས་གྲས་སྒེའུ་ཁུང་གི་མཐེབ་གནོན་རྣམ་པ་གནོན་པ།"

msgid "Click-type window geometry"
msgstr "རིགས་གྲས་སྒེའུ་ཁུང་གི་དབྱིབས་རྩིས་ཆེ་ཆུང་གནོན་པ།"

msgid "Click-type window orientation"
msgstr "རིགས་གྲས་སྒེའུ་ཁུང་གི་ཁ་ཕྱོགས་གནོན་པ།"

msgid "Click-type window style"
msgstr "རིགས་གྲས་སྒེའུ་ཁུང་གི་རྣམ་པ་གནོན་པ།"

msgid "Double Click"
msgstr "ཉིས་རྡེབ།"

msgid "Drag"
msgstr "འདྲུད་འཐེན།"

msgid "Enable dwell click"
msgstr "ལྡིང་འཁོར་རྡེབ་གནོན་སྤྱོད་འགོ་འཛུགས་པ།"

msgid "Enable simulated secondary click"
msgstr "བཀོལ་སྤྱོད་ལད་ཟློས་མཐེབ་མནོན་ཕལ་བ་སྤྱོད་འགོ་འཛུགས་པ།"

msgid "Failed to Display Help"
msgstr "རོགས་རམ་ཁ་འབྱེད་ཕམ་པ།"

msgid "Hide the click-type window"
msgstr "རྡེབ་གནོན་རིགས་གྲས་སྒེའུ་ཁུང་སྦེད་པ།"

msgid "Horizontal"
msgstr "ཆུ་སྙོམས།"

msgid "Hover Click"
msgstr "དཔྱང་འཕྱོ་རྡེབ་གནོན།"

msgid "Icons only"
msgstr "རིས་རྟགས་ཁོ་ན།"

msgid "Ignore small pointer movements"
msgstr "ཙིག་རྟགས་ཀྱི་ཕྱོགས་སྟོན་ཅུང་ཡང་བའི་སྒུལ་བསྐྱོད་སྣང་མེད་བྱེད་པ།"

msgid "Orientation"
msgstr "ཁ་ཕྱོགས།"

msgid "Orientation of the click-type window."
msgstr "རིགས་གྲས་སྒེའུ་ཁུང་གི་ཁ་ཕྱོགས་གནོན་པ།"

msgid "Secondary Click"
msgstr "མཐེབ་གནོན་ཕལ་བ་(གཡས་མཐེབ་)གནོན་པ།"

msgid "Set the active dwell mode"
msgstr "བྱེད་སྒོའི་ལྡིང་འཁོར་རྣམ་པ་སྒྲིག་འགོད།"

msgid "Shut down mousetweaks"
msgstr "mozetweaksསྒོ་རྒྱག་པ།"

msgid "Single Click"
msgstr "ཆིག་རྡེབ།"

msgid ""
"Size and position of the click-type window. The format is a standard X "
"Window System geometry string."
msgstr ""
"རིགས་གྲས་སྒེའུ་ཁུང་གི་ཆེ་ཆུང་དང་གནས་ཡུལ་གནོན་པ། "
"རྣམ་གཞག་ནི་ཚད་ལྡན་གྱི་Xསྒེའུ་ཁུང་རྒྱུད་ཁོངས་གི་དབྱིབས་རྩིས་ཆེ་ཆུང་གི་ཡིག་རྟགས་ཕྲེང་ཡིན།"

msgid "Start mousetweaks as a daemon"
msgstr "ལྟ་སྲུང་གི་འཕེལ་རིམ་བྱས་ནས་mousetweaksའགོ་སློང་བ།"

msgid "Start mousetweaks in login mode"
msgstr "ཐོ་འགོད་རྣམ་པའི་ཐོག་ནས་mozetweaksའགོ་སློང་པ།"

msgid "Text and Icons"
msgstr "ཡིག་དེབ་དང་རིས་རྟགས།"

msgid "Time to wait before a dwell click"
msgstr "ལྡིང་འཁོར་རྡེབ་གནོན་མ་བྱས་གོང་གི་ངང་སྒུག་དུས་ཚོད།"

msgid "Time to wait before a simulated secondary click"
msgstr "མཐེབ་མནོན་ཕལ་བ་（གཡས་མཐེབ་）རྡེབ་མནོན་གོང་གི་ངང་སྒུག་དུས་ཚོད་ལད་ཟློས།"

msgid "Vertical"
msgstr "དྲང་འཕྱངས།"
