msgid "0-n\tReplace with one of the suggested words.\n"
msgstr "0-nattཨང་ཀིའི་མཐེབ་དེ་རེའུ་མིག་ཁྲོད་ཀྱི་ཐ་སྙད་ཅིག་ཏུ་བརྗེ་བ། \n"

msgid "?\tShow this help screen.\n"
msgstr ""
"\n"
"?\tརོགས་རམ་བརྙན་ཡོལ་འདི་འཆར་བ། \n"

msgid "A\tAccept the word for the rest of this session.\n"
msgstr "A\tམཐེབ་ཀྱིས་དུམ་མཚམས་འདིར་ཚིག་འདི་དང་ལེན་བྱེད་པ།\n"

msgid "AVAILABLE DICTIONARIES (path is not mandatory for -d option):\n"
msgstr "སྤྱོད་རུང་ཚིག་མཛོད།（འགྲོ་ལམ་གྱིས་བཙན་ཤེད་དུ་-dའདེམ་ཚན་མི་སྤྱོད།）\n"

msgid "Are you sure you want to throw away your changes? "
msgstr "ཁྱོད་ཀྱིས་བཟོ་བཅོས་བྱེད་མཚམས་འཇོག་རྒྱུ་གཏན་འཁེལ་ལམ།"

msgid "Bug reports: http://hunspell.sourceforge.net\n"
msgstr "ནོར་འཁྲུལ་གྱི་སྙན་ཞུ།： http://hunspell.sourceforge.net\n"

msgid "Can't open %s.\n"
msgstr "%sཁ་ཕྱེ་ཐབས་བྲལ། \n"

msgid "Cannot update personal dictionary."
msgstr "སྒེར་གྱི་ཚིག་མཛོད་དུ་རིམ་སྤོར་བྱ་ཐབས་བྲལ།"

msgid "FORBIDDEN!"
msgstr "གཏན་འགོག"

msgid "Hunspell has been compiled without Ncurses user interface.\n"
msgstr ""
"Hunspllསྤྱད་ཟིན་པའི་Ncursesསྤྱོད་མཁན་མཐུད་མཚམས་སྒྲིག་སྒྱུར་བྱས་ཚར་མེད། \n"

msgid "I\tAccept the word, and put it in your private dictionary.\n"
msgstr ""
"I\tམཐེབ་ཀྱིས་ཚིག་དེ་དང་ལེན་བྱེད་པ་མ་ཟད། དེ་མི་སྒེར་གྱི་ཚིག་མཛོད་ནང་དུ་བཅུག "
"\n"

msgid "Line %d: %s -> "
msgstr "ཕྲེང་། %d: %s ->"

msgid "Model word (a similar dictionary word): "
msgstr "Model word（ཚིག་མཛོད་ཀྱི་ཐ་སྙད་དང་འདྲ་བ་）"

msgid "Model word must be in the dictionary. Press any key!"
msgstr "Model wordངེས་པར་ཚིག་མཛོད་ནང་དུ་དགོས། མཐེབ་གང་རུང་ཞིག་ནོན།"

msgid "New word (stem): "
msgstr "ཐ་སྙད་གསར་བ།（ཚིག་སྙིང་།）"

msgid "Q\tQuit immediately. Asks for confirmation. Leaves file unchanged.\n"
msgstr "Q\tམཐེབ་ཀྱིས་ཕྱིར་འཐེན་མིན་འདྲི། མིག་སྔར་ཡིག་ཆ་ཉར་ཚགས་བྱས་མེད། \n"

msgid "R\tReplace the misspelled word completely.\n"
msgstr "R\t མཐེབ་ཀྱིས་ནོར་འཁྲུལ་གྱི་མིང་ཚིག་བརྗེས་ཟིན། \n"

msgid "Replace with: "
msgstr "བརྗེ་བ།:"

msgid "Space\tAccept the word this time only.\n"
msgstr "Space\tསྟོང་མཐེབ་ཀྱིས་ཚིག་འདི་ཐེངས་གཅིག་སྣང་མེད་བྱས། \n"

msgid "U\tAccept and add lowercase version to private dictionary.\n"
msgstr ""
"U\tམཐེབ་ཀྱིས་ཚིག་འདི་དང་ལེན་བྱས་པ་མ་ཟད།ཆུང་བྲིས་རྣམ་བཞག་མི་སྒེར་གྱི་ཚིག་མཛོད་ནང་དུ་བཅུག"
" \n"

msgid "Usage: hunspell [OPTION]... [FILE]...\n"
msgstr "སྤྱོད་སྟངས།: hunspell [འདེམ་བྱང་]... [ཡིག་ཆ]...\n"

msgid ""
"X\tWrite the rest of this file, ignoring misspellings, and start next "
"file.\n"
msgstr ""
"X\tམཐེབ་ཀྱིས་རྗེས་མའི་ནང་དོན་སྣང་མེད་བྱས་པ་མ་ཟད།ཡིག་ཆ་ཉར་ཚགས་ཀྱིས་ཡིག་ཆ་རྗེས་མའི་མགོ་བརྩམས།"
" \n"

msgid "^Z\tSuspend program. Restart with fg command.\n"
msgstr ""
"^Z\tCtrl＋Zམཐེབ་ཀྱི་སྐབས་སྡོད་དེ་fgབཀའ་བརྡ་སྤྱད་དེ་མུ་མཐུད་དུ་འཁོར་སྐྱོད་བྱེད་དུ་འཇུག་པ།"
" \n"

msgid "a"
msgstr "\"a\""

msgid "error - %s exceeds dictionary limit.\n"
msgstr "ནོར་འཁྲུལ།- %s ཚིག་མཛོད་ཀྱི་ཚད་བཀག་ལས་བརྒལ་བ། \n"

msgid "error - iconv_open: %s -> %s\n"
msgstr "ནོར་འཁྲུལ།- iconv_open: %s -> %s\n"

msgid "error - iconv_open: UTF-8 -> %s\n"
msgstr "ནོར་འཁྲུལ།- iconv_open: UTF-8 -> %s\\"

msgid "error - missing HOME variable\n"
msgstr "ནོར་འཁྲུལ། -  HOME འགྱུར་ཚད་ཆད་པ།\n"

msgid "x"
msgstr "དྲན་ཤེས།"

msgid "y"
msgstr "ཡིན།"
