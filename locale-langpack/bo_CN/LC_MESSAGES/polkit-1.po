msgid "Authentication is needed to run `$(program)' as the super user"
msgstr ""
"རིམ་འདས་སྤྱོད་མཁན་གྱི་ཐོབ་ཐང་གིས་“$(program)” བྱ་རིམ་འཁོར་སྐྱོད་འདོད་ན། "
"ཁྱེད་ངེས་པར་དུ་ར་སྤྲོད་བརྒྱུད་དགོས།"

msgid "Authentication is needed to run `$(program)' as user $(user)"
msgstr ""
"$(user)ཡི་ཐོབ་ཐང་གིས་“$(program)”བྱ་རིམ་འཁོར་སྐྱོད་བྱ་དགོས་ན། "
"ཁྱེད་ཀྱིས་ངེས་པར་དུ་ར་སྤྲོད་བརྒྱུད་དགོས།"

msgid "Authentication is required to configure lock down policy"
msgstr "སྒྲོག་པའི་སྒྲིག་སྲོལ་སྒྲིག་འགོད་བྱ་དགོས་ན།"

msgid "Authentication is required to run a program as another user"
msgstr ""
"ཐོབ་ཐང་གཞན་དག་གིས་བྱ་རིམ་འཁོར་སྐྱོད་བྱ་དགོས་ན། "
"ཁྱེད་ཀྱིས་ངེས་པར་དུ་ར་སྤྲོད་བརྒྱུད་དགོས།"

msgid "Configure lock down for an action"
msgstr "འགུལ་སྟངས་ཤིག་གི་སྒྲོག་པའི་རྣམ་པ་སྒྲིག་འགོད།"

msgid "Run programs as another user"
msgstr "ཐོབ་ཐང་གཞན་དག་གིས་བྱ་རིམ་འཁོར་སྐྱོད་བྱ།"

msgid "Run the PolicyKit example program Frobnicate"
msgstr " PolicyKit ཡི་དཔེ་མཚོན་བྱ་རིམ་ - Frobnicate འཁོར་སྐྱོད་བྱ།"
