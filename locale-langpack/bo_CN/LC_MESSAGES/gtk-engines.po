msgid "3d-ish (button)"
msgstr "3Dརྣམ་པ།(མཐེབ་གནོན།)"

msgid "3d-ish (gradient)"
msgstr "3Dརྣམ་པ།(ཐེམ་ཚད།)"

msgid "Animations"
msgstr "འགུལ་རིས།"

msgid "Arrow"
msgstr "མདའ་རྩེ།"

msgid "Cell Indicator Size"
msgstr "སྡེ་ཚན་སྟོན་ཆས་ཆེ་ཆུང་།"

msgid "Classic"
msgstr "གསུང་རབ་འཆར་ངོས།"

msgid "Colorize Scrollbar"
msgstr "འགྲིལ་བྱང་གི་ཁ་དོག་སྒྲིག་པ།"

msgid "Contrast"
msgstr "བསྡུར་ཚད།"

msgid "Disable focus drawing"
msgstr "མདོ་ཚེག་འབྲི་བ་འགོག་པ།"

msgid "Dot"
msgstr "ཚེག"

msgid "Edge Thickness"
msgstr "མཐའ་ཁུལ་མཐུག་ཚད།"

msgid "Enable Animations on Progressbars"
msgstr "སྒྲུབ་ཚད་བྱང་གི་ཕྱེ་བའི་འགུལ་རིས་ཀྱི་ཕན་འབྲས།"

msgid "Flat"
msgstr "ཁོད་སྙོམས།"

msgid "Full"
msgstr "ཆ་ཚང་།"

msgid "Glossy"
msgstr "བཀྲག་མདངས།"

msgid "Gummy"
msgstr "འབྱར་བའི་རང་བཞིན།"

msgid "Handlebox Marks"
msgstr "འཕྱོ་རུང་སྒེའུ་ཁུང་རྟགས།"

msgid "If set, button corners are rounded"
msgstr "གལ་ཏེ་ཁ་ཕྱེས་ན།མཐེབ་གནོན་མཐའ་དེ་མགོ་བདེ་ཞིག་ཏུ་འགྱུར་ངེས།"

msgid "Inset"
msgstr "བར་འཇུག་རི་མོ།"

msgid "Inverted"
msgstr "ལྡོག་འཁོར།"

msgid "Inverted Dot"
msgstr "ལྡོག་འཁོར་ཚེག"

msgid "Inverted Slash"
msgstr "ལྡོག་འཁོར་གསེག་ཐིག"

msgid "Mark Type 1"
msgstr "རྟགས་ཀྱི་རིགས་གྲས།1"

msgid "Mark Type for Scrollbar Buttons"
msgstr "འདེད་མའི་མཐེབ་གནོན་གྱི་མཚོན་རྟགས་རིགས་གྲས།"

msgid "Mark Type for Scrollbar Handles, Handleboxes, etc"
msgstr ""
"འདེད་མའི་ཡུ་བ། འཕྱོ་རུང་སྒེའུ་ཁུང་དང་གཞན་དག་གི་མཚོན་རྟགས་ཀྱི་རིགས་གྲས།"

msgid "Menubar Style"
msgstr "འདེམས་བྱང་སྡེའི་བཟོ་ཉམས།"

msgid "None"
msgstr "བདེ་འཇགས་སྲུང་སྐྱོང་མེད་པ།"

msgid "Nothing"
msgstr "མེད།"

msgid "Paned Dots"
msgstr "དྲ་མིག་ཅན་གྱི་དྲ་ཚིགས།"

msgid "Radius"
msgstr "ཚངས་ཕྱེད།"

msgid "Rectangle"
msgstr "ཟུར་གྲུ་བཞི་དབྱིབས།"

msgid "Relief Style"
msgstr "ལྷོད་གཡེང་གི་བཟོ་ཉམས།"

msgid "Requires style Glossy or Gummy"
msgstr "འོད་མདངས་སམ་འབྱར་གཤིས་བཟོ་ཉམས་ཀྱི་རེ་འདུན།"

msgid "Rounded Buttons"
msgstr "མགོ་བདེ་མཐེབ་གནོན།"

msgid "Scrollbar Color"
msgstr "འདེད་མའི་ཁ་དོག"

msgid "Scrollbar Marks"
msgstr "འདེད་མའི་རྟགས།"

msgid "Scrollbar Type"
msgstr "འདེད་མའི་རིགས་གྲས།"

msgid "Scrollbutton Marks"
msgstr "འདེད་མའི་མཐེབ་གནོན་རྟགས།"

msgid "Sets the Color of Scrollbars"
msgstr "འདེད་མའི་ཁ་དོག་སྒྲིག་པ།"

msgid "Shadow"
msgstr "གྲིབ་ནག"

msgid "Shaped"
msgstr "དབྱིབས།"

msgid "Size of check- and radiobuttons inside treeviews. (Bug #351764)"
msgstr ""
"སྡོང་དབྱིབས་མངོན་འཆར་ཁྲོད་མང་འདེམས་མཐེབ་གནོན་དང་རྐྱང་འདེམས་མཐེབ་གནོན་གྱི་ཆེ་ཆུང་།"
" (Bug #351764)"

msgid "Slash"
msgstr "གསེག་ཐིག"

msgid "Some"
msgstr "ཁ་ཤས།"

msgid "Style"
msgstr "ཉམས་འགྱུར།"

msgid ""
"This option allows to disable the focus drawing. The primary purpose is to "
"create screenshots for documentation."
msgstr ""
"འདེམ་བྱང་འདིས་རི་མོའི་མདོ་ཚེག་འབྲི་བ་བཀག་ཆོག "
"དེའི་དམིགས་ཡུལ་གཙོ་བོ་ནི་ཡིག་ཚགས་ཀྱི་བརྙན་ཡོལ་བཅད་རིས་བཟོ་རྒྱུ་དེ་རེད།"

msgid "Toolbar Style"
msgstr "ལག་ཆའི་སྡེ་གྱི་རྣམ་པ།"
