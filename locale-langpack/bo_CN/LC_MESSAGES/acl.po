msgid "\t%s -B pathname...\n"
msgstr "\t%s -B ཡི་འགྲོ་ལམ་མིང་...\n"

msgid "\t%s -b acl dacl pathname...\n"
msgstr "\t%s -b acl dacl ཡི་འགྲོ་ལམ་མིང་...\n"

msgid "\t%s -l pathname...\t[not IRIX compatible]\n"
msgstr "\t%s -l ཡི་འགྲོ་ལམ་མིང་...\t[ IRIXདམ་མི་འཆམ]\n"

msgid "  -d, --default           display the default access control list\n"
msgstr " -d，defaultསོར་བཞག་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་འཆར་བ།"

msgid "%s %s -- get file access control lists\n"
msgstr "%s %s -- ཡིག་ཆའི་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་ཐོན་པ།\n"

msgid "%s %s -- set file access control lists\n"
msgstr "%s %s -- ཡིག་ཆའི་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་སྒྲིག་འགོད་བྱེད་པ།\n"

msgid "%s: %s in line %d of file %s\n"
msgstr ""
"%s: %s ནི་འཕྲེད་ %d རུ་ཡོད།ཡིག་ཆ %s\n"
"ཡིན།"

msgid "%s: %s in line %d of standard input\n"
msgstr "%s: %s ནི་འཕྲེད་ %d རུ་ཚད་ལྡན་གྱིས་ཕྱིར་འདོན་པ།\n"

msgid "%s: %s: %s in line %d\n"
msgstr "%s: %s: %s ནི་འཕྲེད་ %dརུ་ཡོད།\n"

msgid "%s: %s: Cannot change mode: %s\n"
msgstr "%s：%s：རྣམ་པ་སྒྱུར་མི་ཐུབ།%s\n"

msgid "%s: %s: Cannot change owner/group: %s\n"
msgstr "%s: %s: ཡོད་མཁན་ནམ་ཚོ་སྒྱུར་མི་ཐུབ།: %s\n"

msgid "%s: %s: Malformed access ACL `%s': %s at entry %d\n"
msgstr "%s: %s: རྒྱུན་འགལ་གྱི་ལྟ་སྤྱོད་རེའུ་མིག \"%s\": %sརུ་%dགནས།\n"

msgid "%s: %s: Malformed default ACL `%s': %s at entry %d\n"
msgstr ""
"%s: %s: རྒྱུན་འགལ་གྱི་སོར་བཞག་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག \"%s\": "
"%sརུ་%dགནས།\n"

msgid "%s: %s: No filename found in line %d, aborting\n"
msgstr "%s: %s: འཕྲེད་ %dརུ་ཡིག་ཆའི་མིང་མ་རྙེད། ཕྱིར་འདོན།\n"

msgid "%s: %s: Only directories can have default ACLs\n"
msgstr "%s: %s: དཀར་ཆག་ཁོ་ན་ལ་སོར་བཞག་ཚོད་འཛིན་རེའུ་མིག་གི་རང་བཞིན་ཡོད་པ།\n"

msgid "%s: No filename found in line %d of standard input, aborting\n"
msgstr "%s:ཚད་ལྡན་ནང་འཇུག་གི་འཕྲེད་ %dརུ་ཡིག་ཆའི་མིང་མ་རྙེད། ཕྱིར་འདོན།\n"

msgid "%s: Option -%c incomplete\n"
msgstr "%s: འདེན་ཚན -%cད་དངོས་འགྱུར་བྱུང་མེད།\n"

msgid "%s: Option -%c: %s near character %d\n"
msgstr "%s: འདེན་ཚན -%c: %s ཡིག་རྟགས་ %dལ་ཉེ།\n"

msgid "%s: Removing leading '/' from absolute path names\n"
msgstr "%s: བལྟོས་མེད་འགྲོ་ལམ་མིང་མཇུག་ཏུ་ཨང་རྟགས\" / \"འདོར།"

msgid "%s: Standard input: %s\n"
msgstr "%s: ཚད་ལྡན་ནང་འཇུག %s\n"

msgid "%s: access ACL '%s': %s at entry %d\n"
msgstr "%s: ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག'%s': %sས་བཅད་དུ%dཡོད།\n"

msgid "%s: cannot get access ACL on '%s': %s\n"
msgstr "%s: ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་ཐོབ་ཐབས་མེད་ས '%s': %s\n"

msgid "%s: cannot get access ACL text on '%s': %s\n"
msgstr "%s: ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་གི་ཡིག་རྐྱང་ཐོབ་ཐབས་མེད་ས'%s': %s\n"

msgid "%s: cannot get default ACL on '%s': %s\n"
msgstr "%s: སོར་བཞག་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་ཐོབ་ཐབས་མེད་ས'%s': %s\n"

msgid "%s: cannot get default ACL text on '%s': %s\n"
msgstr ""
"%s: སོར་བཞག་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་གི་ཡིག་རྐྱང་ཐོབ་ཐབས་མེད་ས'%s': %s\n"

msgid "%s: cannot set access acl on \"%s\": %s\n"
msgstr ""
"%s:ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་ སྒྲིག་ཐབས་མེད་ས \\%s\": %s\n"
"\""

msgid "%s: cannot set default acl on \"%s\": %s\n"
msgstr ""
"%s: སོར་བཞག་ལྟ་སྤྱོད་ཚོད་འཛིན་རེའུ་མིག་སྒྲིག་ཐབས་མེད་ས་ \\%s\": %s\n"
"\""

msgid "%s: error removing access acl on \"%s\": %s\n"
msgstr "%s：“%s”ཡི་ACLསྐྱོན་འབྱུང་།：%s\\འདོར་བ།n"

msgid "%s: error removing default acl on \"%s\": %s\n"
msgstr "%s：“%s”ཡི་སོར་བཞག་ACLUསྐྱོན་འབྱུང་འདོར་བ：%s\n"

msgid "%s: malloc failed: %s\n"
msgstr "%s：ནང་གསོག་བགོད་པ་ཕམ་པ：%s\n"

msgid "%s: opendir failed: %s\n"
msgstr "%s：དཀར་ཆག་ཁ་འབྱེད་ཕམ་པ：%s\n"

msgid "Duplicate entries"
msgstr "བསྐྱར་ཟློས་ཀྱི་ས་བཅད།"

msgid "Invalid entry type"
msgstr "ནུས་མེད་ཀྱི་ས་བཅད་རིགས་གྲས།"

msgid "Missing or wrong entry"
msgstr "ས་བཅད་བོར་བའམ་ནོར་པ།"

msgid "Multiple entries of same type"
msgstr "རིགས་གྲས་མཐུན་པ་མང་པོའི་ས་བཅད་ཐོན་པ།"

msgid "Try `%s --help' for more information.\n"
msgstr "བཀའ་བརྡ་“%s --help”ཡིས་ཆ་འཕྲིན་དེ་ལས་མང་ཐོབ་པར་ཚོད་ལྟ།\n"

msgid "Usage:\n"
msgstr "སྤྱོད་སྟངས།：\n"

msgid "Usage: %s %s\n"
msgstr "སྤྱོད་སྟངས %s %s\n"

msgid "Usage: %s [-%s] file ...\n"
msgstr "སྤྱོད་ཐབས།： %s  [-%s]  ཡིག་ཆ་ ...\n"

msgid "preserving permissions for %s"
msgstr " %sཡི་དབང་ཚད་ཉར་བཞིན་པ།"

msgid "setting permissions for %s"
msgstr "%s ཡི་དབང་ཚད་སྒྲིག་པ"
