msgid "Are you sure you want to close all programs and restart the computer?"
msgstr ""
"ཁྱོད་ཀྱིས་བྱ་རིམ་ཚང་མ་སྒོ་བརྒྱབས་ནས་རྩིས་འཁོར་བསྐྱར་འགོ་སློང་རྒྱུ་གཏན་ཁེལ་ལམ།"

msgid "Are you sure you want to close all programs and shutdown the computer?"
msgstr ""
"ཁྱེད་ཀྱིས་བྱ་རིམ་ཚང་མ་སྒོ་བརྒྱབས་ནས་རྩིས་འཁོར་སྒོ་རྒྱག་རྒྱུ་གཏན་ཁེལ་ལམ།"

msgid "Cancel"
msgstr " དོར་བ།"

msgid "Hibernate"
msgstr "མལ་གསོ།"

msgid "Keyboard Layout"
msgstr "མཐེབ་གཞོང་བཀོད་པ།"

msgid "Language"
msgstr "སྐད་བརྡ།"

msgid "Options"
msgstr "འདེམས་ཚན།"

msgid "Restart"
msgstr "བསྐྱར་སློང་།"

msgid "Session"
msgstr "གླེང་མོལ།"

msgid "Shutdown"
msgstr "ཁ་རྒྱག"

msgid "Suspend"
msgstr "འགེལ་འཇོག"
