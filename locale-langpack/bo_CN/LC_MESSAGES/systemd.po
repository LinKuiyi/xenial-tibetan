msgid "Download a VM or container image"
msgstr "རྟོག་བཟོའི་འཕྲུལ་ཆས་སམ་སྣོད་ཆས་སྣང་བརྙན་ཕབ་ལེན་བྱེད་པ།"

msgid "Export a VM or container image"
msgstr "རྟོག་བཟོའི་འཕྲུལ་ཆས་སམ་སྣོད་ཆས་སྣང་བརྙན་ཕྱིར་འདྲེན་བྱེད་པ།"

msgid "Hibernate the system"
msgstr "མལ་གསོ།"

msgid "Import a VM or container image"
msgstr "རྟོག་བཟོའི་འཕྲུལ་ཆས་སམ་སྣོད་ཆས་སྣང་བརྙན་ནང་འདྲེན།"

msgid "Manage local virtual machines and containers"
msgstr "རྟོག་བཟོའི་འཕྲུལ་ཆས་སམ་སྣོད་ཆས་དོ་དམ་བྱེད་པ།"

msgid "Power off the system"
msgstr "རྒྱུད་ཁོངས་སྒོ་རྒྱག་པ།"

msgid "Reload the systemd state"
msgstr "systemd རྣམ་པ་རུ་བསྐྱར་དུ་འགེལ་འཇུག་བྱེད་པ།"

msgid "Set host name"
msgstr "གཙོ་འཁོར་མིང་སྒྲིག་འགོད།"

msgid "Set machine information"
msgstr "འཕྲུལ་འཁོར་ཆ་འཕྲིན་སྒྲིག་འགོད།"

msgid "Set system time"
msgstr "རྒྱུད་ཁོངས་དུས་ཚོད་སྒྲིག་འགོད།"

msgid "Set system timezone"
msgstr "རྒྱུད་ཁོངས་དུས་ཁུལ་སྒྲིག་འགོད།"

msgid "Suspend the system"
msgstr "རྒྱུད་ཁོངས་འགེལ་བ།"

msgid "Turn network time synchronization on or off"
msgstr "དྲ་རྒྱའི་དུས་ཚོད་དུས་མཉམ་ཁ་ཕྱེ་བའམ་ཁ་རྒྱག་པ།"
