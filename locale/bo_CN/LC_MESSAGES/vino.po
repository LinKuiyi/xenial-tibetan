"རྒྱང་རིང་སྤྱོད་མཁན་གྱིས་ཁྱེད་ཀྱི་ཙིག་རྟགས་དང་མཐེབ་གཞོང་ཚོད་འཛིན་ཐུབ་པ།"

msgid "Require encryption"
msgstr "གསང་སྡོམ་བྱེད་དགོས།"

msgid ""
"Run 'vino-passwd --help' to see a full list of available command line "
"options"
msgstr ""
" “vino-passwd "
"--help”འཁོར་སྐྱོད་བྱས་ནས་སྤྱོད་རུང་བཀའ་བརྡ་ཕྲེང་གི་འདེམས་ཚན་གྱི་རེའུ་མིག་ཧྲིལ་པོར་ལྟ་བ།"

msgid ""
"Run 'vino-server --help' to see a full list of available command line "
"options"
msgstr ""
" “vino-passwd "
"--help”འཁོར་སྐྱོད་བྱས་ནས་སྤྱོད་རུང་བཀའ་བརྡ་ཕྲེང་གི་འདེམས་ཚན་གྱི་རེའུ་མིག་ཧྲིལ་པོར་ལྟ་བ།"

msgid "Screen"
msgstr "བརྙན་ཡོལ།"

msgid "Security"
msgstr "བདེ་འཇགས།"

msgid "Session management options:"
