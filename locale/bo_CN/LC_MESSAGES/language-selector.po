msgid "%s, %s"
msgstr "%s，%s"

msgid "<b>Example</b>"
msgstr "<b>མིག་དཔེ།</b>"

msgid "<small>Changes take effect next time you log in.</small>"
msgstr ""
"smallབཟོ་བཅོས་བྱས་ན་ཁྱེད་རང་ཐེངས་རྗེས་མར་ཐོ་འགོད་བྱེད་སྐབས་ནུས་པ་ཐོན་གྱི་རེད།"
" (small）ལས་དམའ།"

msgid ""
"<small>Use the same format choice for startup and the login screen.</small>"
msgstr ""
"smallསྒོ་ཕྱེ་པ་དང་ཐོ་འགོད་མཐུད་མཚམས་ཀྱི་རྣམ་པ་གཅིག་འདྲ་རེད། (small）ལས་དམའ།"

msgid ""
"<small>Use the same language choices for startup and the login "
"screen.</small>"
msgstr ""
"smallའགོ་རྩོམ་པའམ་ཐོ་འགོད་མཐུད་མཚམས་སུ་སྐད་རིགས་གཅིག་འདྲ་སྤྱོད་པ། "
"(small）ལས་དམའ།"

msgid "Apply System-Wide"
msgstr "རྒྱུད་ཁོངས་ཧྲིལ་བོར་བཀོལ་སྤྱོད།"

msgid "Chinese (simplified)"
msgstr "རྒྱ་ཡིག(ཟོར་ཡང་གཟུགས།)"

msgid "Chinese (traditional)"
msgstr "རྒྱ་ཡིག(ཟོར་ཡང་མ་ཡིན་པ།)"

msgid "Could not install the full language support"
msgstr "ཆ་ཚང་བའི་སྐད་བརྡའི་རྒྱབ་སྐྱོར་སྒྲིག་འཇུག་བྱེད་ཐབས་བྲལ།"

msgid "Could not install the selected language support"
msgstr "བདམས་ཟིན་པའི་སྐད་བརྡའི་རྒྱབ་སྐྱོར་སྒྲིག་འཇུག་བྱེད་ཐབས་བྲལ།"

msgid "Currency:"
msgstr "དངུལ་ལོར།"

msgid "Details"
msgstr "ཆ་འཕྲིན་ཞིབ་ཕྲ།"

msgid "Display numbers, dates and currency amounts in the usual format for:"
msgstr "གྲངས་ཀ་མངོན་པར།ཚེས་གྲངས་དང་དངུལ་ལོར་གྲངས་འབོར་གྱི་རྣམ་བཞག"

msgid "Failed to authorize to install packages."
msgstr "སྒྲིག་འཇུག་ཐུམ་ར་སྤྲོད་ཕམ་པ།"

msgid "Incomplete Language Support"
msgstr "ཆ་ཚང་མིན་པའི་སྐད་བརྡའི་རྒྱབ་སྐྱོར།"

msgid "Install / Remove Languages..."
msgstr "སྐད་བརྡ་ཁ་སྣོན་ནམ་འཕྲི་དོར།"

msgid "Installed"
msgstr "སྒྲིག་འཇུག་བྱས་ཟིན།"

msgid "Installed Languages"
msgstr "སྒྲིག་འཇུག་བྱས་ཟིན་པའི་སྐད་བརྡ།"

msgid "Keyboard input method system:"
msgstr "མཐེབ་གཞོང་འཇུག་ཐབས་རྒྱུད་ཁོངས།"

msgid "Language"
msgstr "སྐད་བརྡ།"

msgid "Language Support"
msgstr "སྐད་བརྡ་རྒྱབ་སྐྱོར།"

msgid "Language for menus and windows:"
msgstr "འདེམས་བྱང་དང་སྒེའུ་ཁུང་གི་སྐད་བརྡ།"

msgid "No language information available"
msgstr "སྤྱོད་གོ་ཆོད་པའི་སྐད་བརྡའི་ཆ་འཕྲིན།"

msgid "Number:"
msgstr "སྒྲིག་ཨང་།："

msgid "Regional Formats"
msgstr "ས་ཁུལ་གྱི་རྣམ་གཞག"

msgid "Session Restart Required"
msgstr "གླེང་མོལ་བསྐྱར་སློང་བྱ་དགོས།"

msgid "Set system default language"
msgstr "རྒྱུད་ཁོངས་སོར་བཞག་སྐད་བརྡ་སྒྲིག་འགོད།"

msgid "Software database is broken"
msgstr "མཉེན་ཆས་གཞི་གྲངས་མཛོད་ལ་སྐྱོན་ཤོར་བ།"

msgid ""
"Some translations or writing aids available for your chosen languages are "
"not installed yet. Do you want to install them now?"
msgstr ""
"ཁྱེད་ཀྱིས་བདམས་པའི་སྐད་བརྡ་དེའི་ལོ་ཙཱ་དང་རྩོམ་འབྲི་རོགས་རམ་ཁག་ཅིག་ད་རུང་སྒྲིག་འཇུག་བྱས་མེད།"
" ཁྱེད་ཀྱིས་ད་ལྟ་སྒྲིག་འཇུག་བྱེད་འདོད་དམ།"

msgid "System policy prevented setting default language"
msgstr "རྒྱུད་ཁོངས་ཐབས་ཇུས་ཀྱིས་སོར་བཞག་སྐད་བརྡ་སྒྲིག་འགོད་བཀག་པ།"

msgid "The language support is not installed completely"
msgstr "སྐད་བརྡའི་རྒྱབ་སྐྱོར་སྒྲིག་འཇུག་ཆ་ཚང་མེད།"

msgid "The new language settings will take effect once you have logged out."
msgstr ""
"ཁྱེད་རང་གི་རྩིས་ཐོ་ནས་བསུབ་རྗེས་སྐད་བརྡ་གསར་པའི་སྒྲིག་སྒྲིག་འགོད་གཞི་ནས་ནུས་པ་ཐོན་གྱི་རེད།"

msgid ""
"When a language is installed, individual users can choose it in their "
"Language settings."
msgstr ""
"སྐད་བརྡ་ག་གེ་མོ་ཞིག་སྒྲིག་འཇུག་བྱས་རྗེས།སྤྱོད་མཁན་གྱིས་ཁོ་ཚོའི་སྐད་བརྡའི་སྒྲིག་འགོད་ཁྲོད་ནས་གདམ་གསེས་བྱས་ཆོག"

msgid "_Install"
msgstr "སྒྲིག་འཇུག(_I)"

msgid "_Remind Me Later"
msgstr "ཅུང་ཙམ་ནས་གསལ་འདེབས།(_R)"

msgid "_Update"
msgstr "གསར་སྒྱུར། (_U)"

msgid "alternative datadir"
msgstr "འདེམས་རུང་བའི་གཞི་གྲངས་ཡིག་ཁུག"

msgid "check for the given package(s) only -- separate packagenames by comma"
msgstr ""
"སྤྲད་པའི་མཉེན་ཆས་ཐུམ་ཁོ་ན་ཞིབ་བཤེར་བྱེད་པ།--དབྱིན་ཡིག་གི་མཚམས་རྟགས་ཀྱིས་ཐུམ་མིང་གི་བར་འབྱེད་པ།"

msgid "don't verify installed language support"
msgstr "སྒྲིག་འཇུག་བྱས་ཟིན་པའི་སྐད་བརྡའི་རྒྱབ་སྐྱོར་ར་སྤྲོད་མི་བྱེད།"

msgid "none"
msgstr " མེད།"

msgid "output all available language support packages for all languages"
msgstr "སྐད་བརྡ་ཡོངས་ཀྱི་ཕྱིར་འདོན་ལ་སྤྱོད་རུང་བའི་སྐད་བརྡའི་རྒྱབ་སྐྱོར་ཐུམ།"

msgid "show installed packages as well as missing ones"
msgstr "དུས་མཉམ་དུ་སྒྲིག་འཇུག་བྱས་ཟིན་པ་དང་ཆད་པའི་མཉེན་ཆས་ཐུམ་མངོན།"

msgid "target language code"
msgstr "དམིགས་བྱའི་སྐད་བརྡའི་ཚབ་རྟགས།"
